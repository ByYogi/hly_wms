                    },
                    {
                        title: '速度', field: 'SpeedLevel', width: '70px'
                    },
                    {
                        title: '最小库存', field: 'MinStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '最大库存', field: 'MaxStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '安全库存', field: 'StockNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '在库库存', field: 'CurNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
