    <script src="../JS/easy/js/ajaxfileupload.js" type="text/javascript"></script>

    <script type="text/javascript">
        let loadingFormData = true;


        //查询基础数据
        function QueryAcceptAddress() {
            var mOpts = $('#dgAcceptAddress').datagrid('options');
            mOpts.url = '../House/houseApi.aspx?method=QueryALLProductPageData';
            $('#dgAcceptAddress').datagrid('load', {
                PID: $("#QAPID").combobox('getValue'),//一级产品
                SID: $('#QASID').combobox('getValue'),
                GoodsCode: $('#QGoodsCode').val(),
                Specs: $('#QSpecs').val(),
                Figure: $('#QFigure').val(),
                DelFlag: "0",
            });
        }
        function OpenSelectProductDlg() {
            $('#dlgSelectProduct').dialog('open').dialog('setTitle', '查询产品基础数据');
            //一级产品
            $('#QAPID').combobox({
                url: '../Product/productApi.aspx?method=QueryALLOneProductType', valueField: 'TypeID', textField: 'TypeName',
                onSelect: function (rec) {
                    $('#QASID').combobox('clear');
                    $('#QASID').combobox({
                        valueField: 'TypeID', textField: 'TypeName', PinyinField: 'PinyinName',
                        url: '../Product/productApi.aspx?method=QueryALLOneProductType&PID=' + rec.TypeID,
                        filter: function (q, row) {
                            var opts = $(this).combobox('options');
                            return row[opts.textField].indexOf(q) >= 0 || row[opts.PinyinField].indexOf(q.toLowerCase()) >= 0;
                        }
                    });
                    $('#QASID').combobox('textbox').bind('focus', function () { $('#QASID').combobox('showPanel'); });
                }
            });
        }
        //导出数据
        function AwbExport() {
            var row = $("#dg").datagrid('getData').rows[0];
            if (row == null) { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '没有数据可以进行导出，请重新查询！', 'warning'); return; }
            $.messager.progress({ msg: '请稍后,正在导出中...' });

            $.ajax({
                url: "houseApi.aspx?method=QuerySafeStockDataForExport",
                data: {
                    //HouseID: $('#HouseID').combobox('getValue'),
                    HouseID: $("#HouseID").combobox('getValues').toString(),//仓库ID
                    //AreaID: $('#AreaID').combobox('getValue'),
                    AreaID: $("#AreaID").combobox('getValues').toString(),
                    Specs: $('#Specs').val(),
                    Figure: $('#Figure').val(),
                    GoodsCode: $('#GoodsCode').val(),
                    ProductCode: $("#AProductCode").val(),
                    PID: $('#APID').combobox('getValue'),
                    SID: $('#ASID').combobox('getValue'),
                    StartDate: $('#StartDate').datebox('getValue'),
                    EndDate: $('#EndDate').datebox('getValue'),
                },
                success: function (text) {
                    $.messager.progress("close");
                    if (text == "OK") {
                        var obj = document.getElementById("<%=btnDerived.ClientID %>"); obj.click();
                    }
                    else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text, 'warning'); }
                }
            });
        }
        //导入
        function AwbImport() {
            $('#dlgImport').dialog('open').dialog('setTitle', '导入安全库存数据');
            showData();
            $('#dgImport').datagrid('loadData', { total: 0, rows: [] });
        }
        //显示DataGrid数据列表
        function showData() {
            var columns = [];
            var HID = "<%=UserInfor.HouseID%>";
            columns.push({ title: '', field: 'ExcelNo', checkbox: true,  width: '35px' });
            columns.push({ title: '所属仓库', field: 'HouseName', width: '65px' });
            columns.push({ title: '产品品牌', field: 'TypeName', width: '65px' });
            columns.push({ title: '产品编码', field: 'ProductCode', width: '85px' });
            columns.push({ title: '产品规格', field: 'Specs', width: '85px' });
            columns.push({ title: '产品花纹', field: 'Figure', width: '55px' });
            columns.push({ title: '货品代码', field: 'GoodsCode', width: '85px' });
            columns.push({ title: '载重', field: 'LoadIndex', width: '55px' });
            columns.push({ title: '速度', field: 'SpeedLevel', width: '55px' });
            columns.push({ title: '月均销量', field: 'AvgSaleNum', width: '65px' });
            columns.push({ title: '最小库存', field: 'MinStock', width: '65px' });
            columns.push({ title: '最大库存', field: 'MaxStock', width: '65px' });
            columns.push({ title: '安全库存', field: 'StockNum', width: '65px' });
            columns.push({ title: '最小库存天数', field: 'MinStockDay', width: '85px' });
            columns.push({ title: '最大库存天数', field: 'MaxStockDay', width: '85px' });
            columns.push({ title: 'OE安全库存数', field: 'OEStock', width: '85px' });
            columns.push({ title: 'YC安全库存数', field: 'HCYCStock', width: '85px' });
            columns.push({ title: 'RE安全库存数', field: 'REStock', width: '85px' });
            columns.push({ title: '隔离库存数', field: 'IsolatedStock', width: '65px' });
            columns.push({ title: '品控库存数', field: 'ControlStock', width: '65px' });

            $('#dgImport').datagrid({
                width: '100%',
                height: '100%',
                title: '', //标题内容
                loadMsg: '数据加载中请稍候...',
                autoRowHeight: false, //行高是否自动
                collapsible: false, //是否可折叠
                pagination: false, //分页是否显示
                rownumbers: true,//显示序号
                pageSize: 12, //每页多少条
                pageList: [12, 20],
                fitColumns: true, //设置为 true，则会自动扩大或缩小列的尺寸以适应网格的宽度并且防止水平滚动
                singleSelect: false, //设置为 true，则只允许选中一行。
                checkOnSelect: false, //如果设置为 true，当用户点击某一行时，则会选中/取消选中复选框。如果设置为 false 时，只有当用户点击了复选框时，才会选中/取消选中复选框
                url: null,
                toolbar: '#dginporttoolbar',
                columns: [columns]
            });
        }
        //保存上传的文件
        function saveFile() {
            var file = $("#fileT").val();
            if (file == null || file == "") {
                // $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择导入文件!', 'info');
                return;
            }
            $('#dgImport').datagrid('loadData', { total: 0, rows: [] });
            ajaxFileUpload();
        }
        //文件上传
        function ajaxFileUpload() {
            $('#ExistCount').val('');
            $.messager.progress({ msg: '请稍后,正在导入中...' });
            $.ajaxFileUpload({
                url: 'houseApi.aspx?method=saveFile',
                secureuri: false,
                fileElementId: 'fileT',
                dataType: 'json',
                success: function (data) {
                    $.messager.progress("close");
                    var val = JSON.parse(data.responseText);
                    var result = val.Result;
                    if (result == true) {
                        var type = val.Type;
                        if (type == 1) {

                            var reg = new RegExp("\r\n", "g");
                            var message = val.Message.replace(reg, "<br>");
                            $.messager.alert('以下Excel数据有误已跳过导入', message, 'warning');
                        }
                        var value = eval(val.Data);
                        $('#dgImport').datagrid('loadData', value);
                        $('#dlgImport').dialog('close');
                        if (val.ExistCount > 0) {
                            $('#ExistCount').val(val.ExistCount);
                        }
                    }
                    else {
                        var message = val.Message;
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', message, 'warning');
                    }
                },
                error: function (data) {
                    $.messager.progress("close");
                    var val = JSON.parse(data.responseText);
                    var result = val.Result;
                    if (result == true) {
                        var type = val.Type;
                        if (type == 1) {

                            var reg = new RegExp("\r\n", "g");
                            var message = val.Message.replace(reg, "<br>");
                            $.messager.alert('以下Excel数据有误已跳过导入', message, 'warning');
                        }
                        var value = eval(val.Data);
                        $('#dgImport').datagrid('loadData', value);
                        if (val.ExistCount > 0) {
                            $('#ExistCount').val(val.ExistCount);
                        }
                    }
                    else {
                        var message = val.Message;
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', message, 'warning');
                    }
                }
            })
            return false;
        }
        function closeDlgStatus() {
            $('#dlgImport').dialog('close');
            $('#dgImport').datagrid('loadData', { total: 0, rows: [] });
            $('#fileT').val("");
        }
        function DelRows() {
            var row = $('#dgImport').datagrid('getSelections');
            if (row) {
                for (var i = 0; i < row.length; i++) {
                    var rowIndex = $('#dgImport').datagrid('getRowIndex', row[i]);
                    $('#dgImport').datagrid('deleteRow', rowIndex);
                }
            }
        }
        //新增
        function addItem() {
            $('#dlgedit').form('clear');
            $('#dlgedit').dialog('open').dialog('setTitle', '新增安全库存');
            $("#eHouseID").combobox("setValue", '<%=UserInfor.HouseID%>');
            $('#eAreaID').combobox('clear');
            var url = '../House/houseApi.aspx?method=QueryALLArea&pid=0&hid=<%=UserInfor.HouseID%>';
            $('#eAreaID').combobox('reload', url);
        }

        //修改
        function editItem() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要修改的数据！', 'warning');
                return;
            }
            if (rows.length > 1) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择一条要修改的数据！', 'warning');
                return;
            }
            var row = rows[0];
            if (row) {
                console.log("加载值",{row});
                var productName = row.TypeName + " " + row.Specs + " " + row.Figure + " " + row.LoadIndex + row.SpeedLevel;
                $('#ProductName').textbox('setValue', productName);
                $('#dlgedit').dialog('open').dialog('setTitle', '修改安全库存配置');

                $('#eAreaID').combobox('clear');
                var url = '../House/houseApi.aspx?method=QueryALLArea&pid=0&hid=' + row.HouseID;
                $('#eAreaID').combobox('reload', url);

                loadingFormData = true;
                $('#fm').form('load', {...row, ProductName: productName});
                loadingFormData = false;
            }
        }

        //导入数据
        function btnSaveDataImprot() {
            var rows = $("#dgImport").datagrid('getData').rows;
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请先导入需要保存的数据！', 'warning');
                return;
            }
            var msg = "确定保存？";
            //var existCount = $('#ExistCount').val();
            //if (existCount > 0) {
            //    msg = "当前系统已存在" + existCount + "条本次导入同期数据，保存将覆盖所有数据，确定保存？";
            //}
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', msg, function (r) {
                if (r) {
                    $.messager.progress({ msg: '请稍后,正在保存中...' });
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: 'houseApi.aspx?method=SaveImportData',
                        type: 'post', dataType: 'json', data: { data: json },
                        success: function (text) {
                            $.messager.progress("close");
                            if (text.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '导入成功!', 'info');
                                 $('#dlgImport').dialog('close');
                                dosearch();
                                $("#fileT").val("");
                                $('#dgImport').datagrid('loadData', { total: 0, rows: [] 
                            });
                    }
                    else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        //保存数据
        function btnSaveData() {
            $.messager.progress({ msg: '请稍后,正在保存中...' });
            $('#fm').form('submit', {
                url: 'houseApi.aspx?method=SaveSafeStockData',
                onSubmit: function () {
                    var check = $(this).form('enableValidation').form('validate');
                    if (!check) { $.messager.progress("close"); }
                    return check;
                },
                success: function (msg) {
                    $.messager.progress("close");
                    var result = eval('(' + msg + ')');
                    if (result.Result) {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存成功!', 'info');
                        $('#dlgedit').dialog('close');
                        dosearch();
                    } else {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存失败：' + result.Message, 'warning');
                    }
                },
            })
        }
        //删除数据
        function DelItem() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要删除的数据！', 'warning');
                return;
            }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定删除？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: 'houseApi.aspx?method=DeleteSafeStockData',
                        type: 'post', dataType: 'json', data: { data: json },
                        success: function (text) {
                            if (text.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '删除成功!', 'info');
                                $('#dg').datagrid('clearSelections');
                                $('#dg').datagrid('reload');
                            }
                            else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        function salesReload(){
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定同步最新销量数据？', function (r) {
                if (r) {
                    $.ajax({
                        url: 'houseApi.aspx?method=SycSaleQty',
                        type: 'post', 
                        success: function (rsps) {
                            rsps = JSON.parse(rsps);
                            if (rsps.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '同步销量数据成功!', 'info');
                                $('#dg').datagrid('reload');
                            }
                            else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', rsps.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        function SaveReturnClient(){
            var row = $('#dgAcceptAddress').datagrid('getSelected');
            if (row == null) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择一条数据！', 'warning');
                return;
            }
            $("#ProductCode").textbox('setValue', row.ProductCode);
            $("#ProductName").textbox('setValue', row.TypeName + " " + row.Specs + " " + row.Figure + " " + row.LoadIndex + row.SpeedLevel);
            $('#TypeName').val(row.TypeName)
            $('#TypeID').val(row.TypeID)
            $('#eSpecs').val(row.Specs)
            $('#eFigure').val(row.Figure)
            $('#eGoodsCode').val(row.GoodsCode)
            $('#eLoadIndex').val(row.LoadIndex)
            $('#eSpeedLevel').val(row.SpeedLevel)
            $('#dlgSelectProduct').dialog('close');
        }
        function maxStockDayChange(){
            if(loadingFormData){
                return;
            }
            // 获取月均销量
            var $avgSaleNum = $('#AvgSaleNum_ipt');
            var $maxStockDay = $('#MaxStockDay_ipt');
            var avgSaleNum = parseFloat($avgSaleNum.numberbox('getValue')) || 0;
            var maxDay = parseFloat($maxStockDay.numberbox('getValue')) || 0;

            // 计算最大库存
            var maxStock = Math.ceil(avgSaleNum / 30 * maxDay);
            // 更新到表单
            $('#fm').form('load', {MaxStock: maxStock});
        }
        function minStockDayChange(){
            if(loadingFormData){
                return;
            }
            // 获取月均销量
            var $avgSaleNum = $('#AvgSaleNum_ipt');
            var $minStockDay = $('#MinStockDay_ipt');

            var avgSaleNum = parseFloat($avgSaleNum.numberbox('getValue')) || 0;
            var minDay = parseFloat($minStockDay.numberbox('getValue')) || 0;

            // 计算最小库存
            var minStock = Math.ceil(avgSaleNum / 30 * minDay);

            // 更新到表单
            $('#fm').form('load', {MinStock: minStock});
        }
        function safeStockChange(){
            if(loadingFormData){
                return;
            }
            var oeStock = parseFloat($('#OEStock_ipt').numberbox('getValue')) || 0;
            var ycStock = parseFloat($('#YCStock_ipt').numberbox('getValue')) || 0;
            var reStock = parseFloat($('#REStock_ipt').numberbox('getValue')) || 0;

            // 累加安全库存数
            var safestock = oeStock + ycStock + reStock;
            // 更新到表单
            $('#fm').form('load', {StockNum: safestock});
        }
        $(function (){
            $('#MinStockDay_ipt').numberbox({
                min: 0,
                precision: 0,
                onChange: function(newValue, oldValue) {
                    if ($(this).data('initialized')) {
                        minStockDayChange()
                    } else {
                        $(this).data('initialized', true);
                    }
                }
            });
            $('#MaxStockDay_ipt').numberbox({
                min: 0,
                precision: 0,
                onChange: function(newValue, oldValue) {
                    if ($(this).data('initialized')) {
                        maxStockDayChange()
                    } else {
                        $(this).data('initialized', true);
                    }
                }
            });

            $('#OEStock_ipt').numberbox({
                min: 0,
                precision: 0,
                onChange: function() {
                    safeStockChange()
                }
            });
            $('#YCStock_ipt').numberbox({
                min: 0,
                precision: 0,
                onChange: function() {
                    safeStockChange()
                }
            });
            $('#REStock_ipt').numberbox({
                min: 0,
                precision: 0,
                onChange: function() {
                    safeStockChange()
                }
            });
        })
    </script>
</asp:Content>
