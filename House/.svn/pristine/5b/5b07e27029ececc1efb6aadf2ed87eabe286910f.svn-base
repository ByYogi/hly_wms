                    {
                        title: '区域大仓', field: 'HouseName', width: '80px'
                    },
                    {
                        title: '所属仓库', field: 'AreaName', width: '80px'
                    },
                    {
                        title: '品牌', field: 'TypeName', width: '80px'
                    },
                    {
                        title: '产品编码', field: 'ProductCode', width: '100px'
                    },
                    {
                        title: '规格', field: 'Specs', width: '100px',sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '花纹', field: 'Figure', width: '100px'
                    },
