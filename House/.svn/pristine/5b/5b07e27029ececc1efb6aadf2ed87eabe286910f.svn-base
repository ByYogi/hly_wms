                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '补货数量', field: 'LessNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '全国在库库存', field: 'TotalNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '云仓销售数量', field: 'WXSaleNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '全渠道销售数量', field: 'SaleNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '全国销售数量', field: 'TotalSaleNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '月均销量', field: 'AvgSaleNum', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '库存度天数', field: 'DOI', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    
                    {
                        title: '最小库存度天数', field: 'MinStockDay', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '最大库存度天数', field: 'MaxStockDay', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: 'OE安全库存数', field: 'OEStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: 'YC安全库存数', field: 'HCYCStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: 'RE安全库存数', field: 'REStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '品控库存数', field: 'ControlStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                    {
                        title: '隔离库存数', field: 'IsolatedStock', align: 'right', width: '100px', sortable: true, sorter: function (a, b) {
                            return (parseInt(a) > parseInt(b) ? 1 : -1);
                        }
                    },
                ]],
                onLoadSuccess: function (data) { },
                onClickRow: function (index, row) {
                    $('#dg').datagrid('clearSelections');
                    $('#dg').datagrid('selectRow', index);
                },
                onDblClickRow: function (index, row) { editItem(); },
                rowStyler: function (index, row) { }
            });

            $('#dgAcceptAddress').datagrid({
                width: '100%',
                height: '480px',
                title: '', //标题内容
                pagination: true, //分页是否显示
                pageSize: 20, //每页多少条
                pageList: [20, 50],
                loadMsg: '数据加载中请稍候...',
                autoRowHeight: false, //行高是否自动
                collapsible: false, //是否可折叠
                fitColumns: false, //设置为 true，则会自动扩大或缩小列的尺寸以适应网格的宽度并且防止水平滚动
                singleSelect: false, //设置为 true，则只允许选中一行。
                checkOnSelect: true, //如果设置为 true，当用户点击某一行时，则会选中/取消选中复选框。如果设置为 false 时，只有当用户点击了复选框时，才会选中/取消选中复选框
                idField: 'SID',
                url: null,
                toolbar: '#AcceptAddressbar',
                columns: [[
                    { title: '', field: 'SID', checkbox: true, width: '30px' },
                    {
                        title: '产品编码', field: 'ProductCode', width: '110px', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '品牌', field: 'TypeName', width: '70px', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '规格', field: 'Specs', width: '100px', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '花纹', field: 'Figure', width: '110px', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '载速', field: 'LoadIndex', width: '70px', formatter: function (value, row) {
                            return "<span title='" + value + row.SpeedLevel + "'>" + value + row.SpeedLevel + "</span>";
                        }
                    },

                    {
                        title: '货品代码', field: 'GoodsCode', width: '200px', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },

                ]],
                onClickRow: function (index, row) {
                    $('#dgAcceptAddress').datagrid('clearSelections');
                    $('#dgAcceptAddress').datagrid('selectRow', index);
                },
                onDblClickRow: function (index, row) {
                    var productName = row.TypeName + " " + row.Specs + " " + row.Figure + " " + row.LoadIndex + row.SpeedLevel;
                    $("#ProductCode").textbox('setValue', row.ProductCode);
                    $("#ProductName").textbox('setValue', productName);
                    $('#TypeName').val(row.TypeName)
                    $('#TypeID').val(row.TypeID)
                    $('#eSpecs').val(row.Specs)
                    $('#eFigure').val(row.Figure)
                    $('#eGoodsCode').val(row.GoodsCode)
                    $('#eLoadIndex').val(row.LoadIndex)
                    $('#eSpeedLevel').val(row.SpeedLevel)
                    $('#dlgSelectProduct').dialog('close');
                }
            })
            //列表回车响应查询
            //$("#saPanelTab").keydown(function (e) { if (e.keyCode == 13) { dosearch(); } });
            //编辑回车响应保存
            //$("#editTab").keydown(function (e) { if (e.keyCode == 13) { saveItem(); } });

            //所在仓库
            $('#HouseID').combobox({
                url: '../House/houseApi.aspx?method=CargoPermisionHouse',
                valueField: 'HouseID', textField: 'Name',
                onSelect: function (rec) {
                    var hids = $("#HouseID").combobox('getValues').toString()
                    $('#AreaID').combobox('clear');
                    var url = '../House/houseApi.aspx?method=QueryALLAreaV2&pid=0&hid=' + hids;
                    $('#AreaID').combobox('reload', url);
                }
            }).combobox('setValue', '<%=UserInfor.HouseID%>');
            //所在仓库
            $('#eHouseID').combobox({
                url: '../House/houseApi.aspx?method=CargoPermisionHouse',
                valueField: 'HouseID', textField: 'Name',
                onSelect: function (rec) {
                    $('#eAreaID').combobox('clear');
                    var url = '../House/houseApi.aspx?method=QueryALLArea&pid=0&hid=' + rec.HouseID;
                    $('#eAreaID').combobox('reload', url);
                }
            });
            //一级产品
            $('#APID').combobox({
                url: '../Product/productApi.aspx?method=QueryALLOneProductType', valueField: 'TypeID', textField: 'TypeName',
                onSelect: function (rec) {
                    $('#ASID').combobox('clear');
                    $('#ASID').combobox({
                        url: '../Product/productApi.aspx?method=QueryALLOneProductType&PID=' + rec.TypeID, valueField: 'TypeID', textField: 'TypeName', AddField: 'PinyinName',
                        filter: function (q, row) {
                            var opts = $(this).combobox('options');
                            return row[opts.textField].indexOf(q) >= 0 || row[opts.AddField].indexOf(q) >= 0;
                        },
                    });
                }
            });
            $('#APID').combobox('textbox').bind('focus', function () { $('#APID').combobox('showPanel'); });
            $('#ASID').combobox('textbox').bind('focus', function () { $('#ASID').combobox('showPanel'); });
            $('#HouseID').combobox('textbox').bind('focus', function () { $('#HouseID').combobox('showPanel'); });
            $('#AreaID').combobox('textbox').bind('focus', function () { $('#AreaID').combobox('showPanel'); });
            $('#eHouseID').combobox('textbox').bind('focus', function () { $('#eHouseID').combobox('showPanel'); });
            $('#eAreaID').combobox('textbox').bind('focus', function () { $('#eAreaID').combobox('showPanel'); });

            
            //所属仓库默认值
            $('#AreaID').combobox({
                url: '../House/houseApi.aspx?method=QueryALLAreaV2&pid=0&hid=' + '<%=UserInfor.HouseID%>',
                onLoadSuccess: function() {
                    var data = $(this).combobox('getData');
                    if (data.length > 0) {
                        $(this).combobox('setValue', data[0].AreaID);
                    }
                }
            })

            var datenow = new Date();
            $('#StartDate').datebox('setValue', getLastDayFormatDate(datenow));
            //$('#StartDate').datebox('setValue', getNowFormatDate(datenow));
            $('#EndDate').datebox('setValue', getNowFormatDate(datenow));
            $('#StartDate').datebox('textbox').bind('focus', function () { $('#StartDate').datebox('showPanel'); });
            $('#EndDate').datebox('textbox').bind('focus', function () { $('#EndDate').datebox('showPanel'); });

        });

        function Getvalue() {
            if ($("#eFigure").combobox('getData').length > 0) {
                $("#eFigure").combobox("setValue", $("#eFigure").combobox('getData')[0].Figure);
            }
            if ($("#eGoodsCode").combobox('getData').length > 0) {
                $("#eGoodsCode").combobox("setValue", $("#eGoodsCode").combobox('getData')[0].GoodsCode);
            }
            if ($("#eLoadIndex").combobox('getData').length > 0) {
                $("#eLoadIndex").combobox("setValue", $("#eLoadIndex").combobox('getData')[0].LoadIndex);
            }
            if ($("#eSpeedLevel").combobox('getData').length > 0) {
                $("#eSpeedLevel").combobox("setValue", $("#eSpeedLevel").combobox('getData')[0].SpeedLevel);
            }
        }
        //查询
        function dosearch() {
            $('#dg').datagrid('clearSelections');
            var gridOpts = $('#dg').datagrid('options');
            gridOpts.url = 'houseApi.aspx?method=QuerySafeStockDataBatch';
            $('#dg').datagrid('load', {
                //HouseID: $('#HouseID').combobox('getValue'),
                HouseID: $("#HouseID").combobox('getValues').toString(),//仓库ID
                //AreaID: $('#AreaID').combobox('getValue'),
                AreaID: $("#AreaID").combobox('getValues').toString(),
                Specs: $("#Specs").val(),
                Figure: $("#Figure").val(),
                GoodsCode: $("#GoodsCode").val(),
                ProductCode: $("#AProductCode").val(),
                PID: $("#APID").combobox('getValue'),//一级产品
                SID: $("#ASID").combobox('getValue'),//二级产品
                StartDate: $('#StartDate').datebox('getValue'),
                EndDate: $('#EndDate').datebox('getValue'),
            });
        }
    
    </script>
</asp:Content>
