                url: null,
                rownumbers: true,
                showFooter: true,
                toolbar: '#toolbar',
                columns: [[
                    {
                        title: '', field: 'SID', checkbox: true, width: '30px', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
