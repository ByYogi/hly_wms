                LODOP.ADD_PRINT_TEXT(106 + ji * 28, 36, 26, 20, js);
                //判断是否选中打印价格勾选框
                if ($('#IsPrintPrice').is(':checked')) {
                    LODOP.ADD_PRINT_RECT(101 + ji * 28, 566, 70, 28, 0, 1);
                    LODOP.ADD_PRINT_TEXT(106 + ji * 28, 582, 56, 20, griddata[i].UnitPrice);
                    LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                    LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                    LODOP.ADD_PRINT_TEXT(106 + ji * 28, 648, 70, 20, griddata[i].Batch);
                    LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                    LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                } else {
                    LODOP.ADD_PRINT_TEXT(106 + ji * 28, 580, 70, 20, griddata[i].Batch);//批次
                    LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                    LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                }
                LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                LODOP.ADD_PRINT_TEXT(106 + ji * 28, 65, 187, 20, griddata[i].TypeName + " " + griddata[i].Specs);//品牌加规格
                LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                LODOP.ADD_PRINT_TEXT(106 + ji * 28, 247, 156, 20, griddata[i].Figure);//花纹
                LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                LODOP.ADD_PRINT_TEXT(106 + ji * 28, 380, 77, 20, griddata[i].LoadIndex + griddata[i].SpeedLevel);//载速
                LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                LODOP.ADD_PRINT_TEXT(106 + ji * 28, 472, 49, 20, griddata[i].Package);//条套
                LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                LODOP.SET_PRINT_STYLEA(0, "Bold", 1);
                LODOP.ADD_PRINT_TEXT(106 + ji * 28, 527, 46, 20, Number(griddata[i].InPiece));//数量
                LODOP.SET_PRINT_STYLEA(0, "FontSize", 10);
                LODOP.SET_PRINT_STYLEA(0, "Bold", 1);

                ji++;
            }
            //LODOP.PREVIEW();

            LODOP.PRINT();
            //LODOP.PRINT_DESIGN();
        }
    </script>
</asp:Content>
