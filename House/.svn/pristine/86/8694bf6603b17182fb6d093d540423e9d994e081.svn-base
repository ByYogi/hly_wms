                                    qyUser = qiye.QueryUserList(new QyUserEntity { CheckRole = AppSet.TwoCheckID, CheckHouseID = ent.HouseID.ToString() });
                                    result.CheckID = AppSet.TwoCheckID;
                                    result.CheckName = AppSet.TwoCheckName;
                                }
