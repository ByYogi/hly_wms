                                        {
                                            cargoGtmcProOrderEntity.DepartDate = Convert.ToString(dts[0]);//发车时间年月日
                                        }
                                    }
                                    if (j == 22)
                                    {
                                        string[] dts = ssd[j].Split(' ');
                                        if (dts.Length > 0)
                                        {
