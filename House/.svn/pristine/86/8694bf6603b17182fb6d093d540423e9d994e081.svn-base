                        msg.Message = "规格：" + Convert.ToString(row["Specs"]).Trim() + "，型号：" + Convert.ToString(row["Model"]).Trim() + "，花纹：" + Convert.ToString(row["Figure"]).Trim() + "库存不足";
                        msg.Result = false;
