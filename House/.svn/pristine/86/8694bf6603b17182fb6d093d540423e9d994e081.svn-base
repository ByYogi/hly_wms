                                        cargoGtmcProOrderEntity.Piece = string.IsNullOrEmpty(Convert.ToString(ssd[j])) ? 0 : Convert.ToInt32(ssd[j]); //看板张数订单数量 
                                    }
                                    if (j == 20)
