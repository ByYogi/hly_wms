                        orderEntity.GoodsCode = Convert.ToString(row["GoodsCode"]);
                        orderEntity.TID = Convert.ToInt64(row["TID"]);
                        orderEntity.ID = Convert.ToString(row["TID"]);
                        orderEntity.HouseID = Convert.ToInt32(row["HouseID"]);
