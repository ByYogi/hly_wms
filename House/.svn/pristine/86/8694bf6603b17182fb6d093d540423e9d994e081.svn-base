                                Common.WriteTextLog("推送成功：" + it.WxName + ent.OrderNo);
                            }

                        }
                        catch (ApplicationException ex)
                        {
                            Common.WriteTextLog("推送失败：" + ent.OrderNo);
                            msg.Result = true;
                        }
                    }
                    #endregion
                }
            }
            catch (Exception)
            {
                msg.Result = true;
            }
            //返回处理结果
            string ress = JSON.Encode(msg);
            Response.Write(ress);
            Response.End();
        }
