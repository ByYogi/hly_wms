                                </table>
                            </fieldset>
                        </td>
                    </tr>
                </table>
            </div>
        </form>
    </div>
    <div id="dlg-buttons">
        <a href="#" class="easyui-linkbutton" iconcls="icon-ok" onclick="saveItem()">&nbsp;保&nbsp;存&nbsp;</a>&nbsp;&nbsp;
        <a href="#" class="easyui-linkbutton" iconcls="icon-cancel" onclick="javascript:$('#dlg').dialog('close')">&nbsp;取&nbsp;消&nbsp;</a>
    </div>

    <script type="text/javascript">
        //新增审批流程信息
        function addItem() {
            $('#dlg').dialog('open').dialog('setTitle', '新增审批流程');
            $('#fm').form('clear');
            $('#DelFlag').combobox('select', '0');

            //$('#HouseID').combobox('setValue', '<%=UserInfor.HouseID%>');
        }
        //修改审批流程
        function editItem() {
            var row = $('#dg').datagrid('getSelected');
            if (row == null || row == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要修改的数据！', 'warning');
                return;
            }
            if (row) {
                $('#dlg').dialog('open').dialog('setTitle', '修改审批流程：' + row.ApproveName);
                $('#fm').form('load', row);
                $('#HouseID').combobox('setValue', row.HouseID);
            }
        }
        //修改仓库信息
        function editItemByID(Did) {
            var row = $("#dg").datagrid('getData').rows[Did];
            if (row) {
                $('#dlg').dialog('open').dialog('setTitle', '修改审批流程：' + row.ApproveName);
                $('#fm').form('load', row);
                $('#HouseID').combobox('setValue', row.HouseID);
            }
        }

        //注销
        function logout() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要注销的数据！', 'warning'); return; }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定注销？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: '../Finance/financeApi.aspx?method=DelFinanceApproveSet&ty=0',
                        type: 'post',
                        dataType: 'json',
                        data: { data: json },
                        success: function (text) {
                            if (text.Result == true) { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '注销成功!', 'info'); $('#dg').datagrid('reload'); }
                            else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning'); }
                        }
                    });
                }
            });
        }
        //恢复
        function renew() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要恢复的数据！', 'warning'); return; }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定恢复？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: '../Finance/financeApi.aspx?method=DelFinanceApproveSet&ty=2',
                        type: 'post',
                        dataType: 'json',
                        data: { data: json },
                        success: function (text) {
                            if (text.Result == true) { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '恢复成功!', 'info'); $('#dg').datagrid('reload'); }
                            else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning'); }
                        }
                    });
                }
            });
        }
        //删除
        function DelItem() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要删除的数据！', 'warning'); return; }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '删除后不能恢复，确定删除？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: '../Finance/financeApi.aspx?method=DelFinanceApproveSet&ty=1',
                        type: 'post',
                        dataType: 'json',
                        data: { data: json },
                        success: function (text) {
                            if (text.Result == true) { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '删除成功!', 'info'); $('#dg').datagrid('reload'); }
                            else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning'); }
                        }
                    });
                }
            });
        }
        //保存审批流程息
        function saveItem() {
            $('#fm').form('submit', {
                url: 'financeApi.aspx?method=SaveFinanceApproveSet',
                onSubmit: function (param) {
                    param.OneCheckName = $('#OneCheckID').combobox('getText');
                    param.TwoCheckName = $('#TwoCheckID').combobox('getText');
                    param.ThreeCheckName = $('#ThreeCheckID').combobox('getText');
                    param.FourCheckName = $('#FourCheckID').combobox('getText');
                    param.FiveCheckName = $('#FiveCheckID').combobox('getText');
                    param.SixCheckName = $('#SixCheckID').combobox('getText');
                    param.CopyCheckName = $('#CopyCheck').combobox('getText');
                    param.CopyUserName = $('#CopyUserID').combobox('getText');
                    param.AutoPassName = $('#AutoPassID').combobox('getText');
                    //param.SevenCheckName = $('#SevenCheckID').combobox('getText');
                    //param.EightCheckName = $('#EightCheckID').combobox('getText');
                    //param.NineCheckName = $('#NineCheckID').combobox('getText');
                    //param.NineCheckName = $('#TenCheckID').combobox('getText');
                    return $(this).form('enableValidation').form('validate');
                },
                success: function (msg) {
                    var result = eval('(' + msg + ')');
                    if (result.Result) {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存成功!', 'info');
                        $('#dlg').dialog('close'); 	// close the dialog
                        dosearch();
                    } else {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存失败：' + result.Message, 'warning');
                    }
                }
            })
        }
    </script>

</asp:Content>
