        protected void btnDerived_Click(object sender, EventArgs e)
        {
            if (OrderDayStatisticsDataExport.Count <= 0) { return; }
            string tname = string.Empty;
            DataTable table = new DataTable();
            table.Columns.Add("序号", typeof(int));
            table.Columns.Add("品牌", typeof(string));
            table.Columns.Add("仓库", typeof(string));
            table.Columns.Add("销量", typeof(int));
            table.Columns.Add("销售SKU", typeof(int));
            table.Columns.Add("目标SKU", typeof(int));
            table.Columns.Add("在库SKU", typeof(int));
            table.Columns.Add("库存", typeof(int));
            table.Columns.Add("SKU满足率", typeof(string));
            table.Columns.Add("动销率", typeof(string));
            table.Columns.Add("库存周转数", typeof(int));
            table.Columns.Add("点击转换率", typeof(string));
            table.Columns.Add("上月销售环比", typeof(string));
            table.Columns.Add("大规格销量率", typeof(string));
            table.Columns.Add("本仓有售SKU缺货率", typeof(string));
            table.Columns.Add("全网有售SKU缺货率", typeof(string));


            int i = 0;
            foreach (var it in OrderDayStatisticsDataExport)
            {
                i++;
                DataRow newRows = table.NewRow();
                newRows["序号"] = i;
                newRows["品牌"] = it.TypeName;
                newRows["仓库"] = it.HouseName;
                newRows["销量"] = it.Piece;
                newRows["销售SKU"] = it.DayStatisticsCount;
                newRows["目标SKU"] = it.StockNum;
                newRows["在库SKU"] = it.ProCount;
                newRows["库存"] = it.ProPiece;
                newRows["SKU满足率"] = it.SatisfactionRate + "%";
                newRows["动销率"] = it.SalesRate + "%";
                newRows["库存周转数"] = it.InventoryTurnover;
                newRows["点击转换率"] = it.ClickConversionRate+"%";
                newRows["上月销售环比"] = it.PreviousMonthRate + "%";
                newRows["大规格销量率"] = it.LargeSpecRatio + "%";
                newRows["本仓有售SKU缺货率"] = it.LocalSkuOutOfStockRate + "%";
                newRows["全网有售SKU缺货率"] = it.GlobalSkuOosRate + "%";
                table.Rows.Add(newRows);

            }
            ToExcel.DataTableToExcel(table, "", "品牌月销售排名统计数"+ DateTime.Now.ToString("yyyyMMdd") );
        }

