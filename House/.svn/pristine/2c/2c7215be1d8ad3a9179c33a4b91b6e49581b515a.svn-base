                    { title: '最大件数', field: 'MaxPiece', width: '60px' },
                    { title: '所在仓库', field: 'FirstAreaName', width: '80px' },
                    { title: '一级区域', field: 'ParentName', width: '80px' },
                    { title: '二级区域', field: 'AreaName', width: '80px' }
                ]]
