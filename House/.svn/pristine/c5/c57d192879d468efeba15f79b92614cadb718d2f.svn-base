        // 切换自配送字段显示/隐藏
        function toggleSelfDeliveryFields(expressCode) {
            if (expressCode === 'THRID_DELIVERY_TMYCCGZPS') {
                $('#selfDeliveryRow').show();
            } else {
                $('#selfDeliveryRow').hide();
                // 清空自配送字段
                $('#DeliveryDriverName').textbox('setValue', '');
                $('#DriverCellphone').textbox('setValue', '');
            }
        }

        // 存储当前选中的订单信息
        var currentDeliveryOrder = null;

        // 手动发货弹窗
        function manualDelivery() {
            var row = $('#dg').datagrid('getSelections');
            if (row == null || row.length == 0) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要发货的订单！', 'warning');
                return;
            }
            
            // 检查是否已推送，已推送的订单禁止手动发货
            if (row[0].IsDeliveryPush == 1) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '该订单已推送，禁止重复发货！', 'warning');
                return;
            }
            
            currentDeliveryOrder = row[0];
            
            // 显示订单信息
            var orderInfo = '业务单号: ' + (currentDeliveryOrder.originBillNo || '') +
                           ' | 客户: ' + (currentDeliveryOrder.customerName || '') +
                           ' | 收件人: ' + (currentDeliveryOrder.consigneeContacts || '');
            $('#deliveryOrderInfo').text(orderInfo);
            
            // 初始化快递公司下拉框
            $('#OpenExpressName').combobox({
                data: expressCompanyData,
                valueField: 'value',
                textField: 'text',
                editable: true,
                onSelect: function(rec) {
                    toggleSelfDeliveryFields(rec.value);
                }
            });
            
            // 绑定 blur 事件来处理输入框失焦时的自动匹配
            $('#OpenExpressName').combobox('textbox').blur(function() {
                var value = $('#OpenExpressName').combobox('getText');
                var data = $('#OpenExpressName').combobox('getData');
                var currentValue = $('#OpenExpressName').combobox('getValue');
                console.log('blur triggered - value:', value, 'data length:', data ? data.length : 0, 'currentValue:', currentValue);
                
                // 如果没有选中值但有输入文本，查找匹配项
                if (value && data && data.length > 0) {
                    for (var i = 0; i < data.length; i++) {
                        // 模糊匹配：文本包含输入内容 或 值包含输入内容
                        if (data[i].text.indexOf(value) !== -1 || data[i].value.toUpperCase().indexOf(value.toUpperCase()) !== -1) {
                            console.log('Matched:', data[i].text, data[i].value);
                            $('#OpenExpressName').combobox('setValue', data[i].value);
                            toggleSelfDeliveryFields(data[i].value);
                            break;
                        }
                    }
                }
            });
            
            // 清空发货相关字段
            $('#OpenExpressName').combobox('setValue', '');
            $('#OpenExpressNum').textbox('setValue', '');
            $('#DeliveryDriverName').textbox('setValue', '');
            $('#DriverCellphone').textbox('setValue', '');
            $('#selfDeliveryRow').hide();
            
            // 初始化明细表格（带发货操作列）
            var deliveryColumns = [];
               deliveryColumns.push({
                title: '操作', field: 'operation', width: '80px', align: 'center',
                formatter: function (val, row, index) {
                    return '<a href="javascript:void(0)" style="width: 100%;" class="easyui-linkbutton" onclick="doDeliveryItem(' + index + ')">发货</a>';
                }
            });
            deliveryColumns.push({ title: 'SKU名称', field: 'skuName', width: '280px' });
            deliveryColumns.push({ title: '需出库数量', field: 'needOutboundNum', width: '80px' });
            deliveryColumns.push({ title: 'SKU订单编码', field: 'skuOrderCode', width: '120px' });
            deliveryColumns.push({
                title: '商品品质等级', field: 'goodsQuality', width: '90px',
                formatter: function (val, row, index) {
                    if (val == "1") { return "好件"; }
                    else if (val == "2") { return "残次品"; }
                    else if (val == "3") { return "坏件"; }
                    else { return "未知等级"; }
                }
            });
            deliveryColumns.push({
                title: '保质期类型', field: 'guaranteePeriod', width: '80px',
                formatter: function (val, row, index) {
                    if (val == "1") { return "正常"; }
                    else if (val == "2") { return "临期"; }
                    else if (val == "4") { return "年份"; }
                    else { return "未知等级"; }
                }
            });
            deliveryColumns.push({ title: 'SKU唯一标识ID', field: 'skuId', width: '100px' });
         
            
            $('#deliveryDetailDg').datagrid({
                width: 900,
                height: 260,
                title: '订单明细',
                loadMsg: '数据加载中请稍候...',
                autoRowHeight: false,
                collapsible: false,
                pagination: false,
                rownumbers: true,
                fitColumns: false,
                singleSelect: true,
                idField: 'id',
                url: 'orderApi.aspx?method=QueryTMallOrderGoods',
                queryParams: { id: currentDeliveryOrder.id },
                columns: [deliveryColumns],
                onLoadSuccess: function (data) {
                    // 渲染操作按钮
                    $('.easyui-linkbutton').linkbutton();
                }
            });
            
            $('#dlgManualDelivery').dialog('open').dialog('setTitle', '手动发货');
        }
        
        // 单行发货操作
        function doDeliveryItem(index) {
            var row = $('#deliveryDetailDg').datagrid('getRows')[index];
            if (!row) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '获取商品信息失败！', 'warning');
                return;
            }
            
            // 验证必填字段
            var expressCode = $('#OpenExpressName').combobox('getValue');
            var expressNum = $('#OpenExpressNum').textbox('getValue');
            if (!expressCode) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择快递公司！', 'warning');
                return;
            }
            if (!expressNum) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请输入快递单号！', 'warning');
                return;
            }
            
            // 如果是自配送，验证自配送字段
            var driverName = '';
            var driverPhone = '';
            if (expressCode === 'THRID_DELIVERY_TMYCCGZPS') {
                driverName = $('#DeliveryDriverName').textbox('getValue');
                driverPhone = $('#DriverCellphone').textbox('getValue');
                if (!driverName) {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请输入自配送联系人姓名！', 'warning');
                    return;
                }
                if (!driverPhone) {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请输入自配送联系人电话！', 'warning');
                    return;
                }
            }

            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>',
                '确定对商品【' + row.skuName + '】进行发货操作？',
                function (r) {
                    if (r) {
                        $.ajax({
                            url: 'orderApi.aspx?method=ManualDeliveryItem',
                            type: 'post',
                            dataType: 'json',
                            data: {
                                orderId: currentDeliveryOrder.id,
                                originBillNo: currentDeliveryOrder.originBillNo,
                                outboundNoticeNo: currentDeliveryOrder.outboundNoticeNo,
                                skuId: row.skuId,
                                skuOrderCode: row.skuOrderCode,
                                itemId: row.id,
                                needOutboundNum: row.needOutboundNum,
                                OpenExpressName: expressCode,
                                OpenExpressNum: expressNum,
                                DeliveryDriverName: driverName,
                                DriverCellphone: driverPhone
                            },
                            success: function (text) {
                                // 清空之前的结果
                                $('#deliveryResult').val('');
                                if (text.Result == true) {
                                    var resultText = '【' + row.skuName + '】发货成功！\n';
                                    resultText += '商品SKU: ' + row.skuOrderCode + '\n';
                                    resultText += '数量: ' + row.needOutboundNum + '\n';
                                    // 如果有额外返回信息，也显示出来
                                    if (text.Message) {
                                        resultText += '返回信息: ' + text.Message + '\n';
                                    }
                                    $('#deliveryResult').val(resultText);
                                    // 刷新明细表格
                                    $('#deliveryDetailDg').datagrid('reload');
                                    // 刷新主表格
                                    $('#dg').datagrid('reload');
                                }
                                else {
                                    var errorText = '【' + row.skuName + '】发货失败！\n';
                                    errorText += '错误信息: ' + (text.Message || '未知错误');
                                    $('#deliveryResult').val(errorText);
                                }
                            },
                            error: function() {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请求失败，请稍后重试', 'error');
                            }
                        });
                    }
                }
            );
        }
        //导出数据
        function AwbExport() {
            var obj = document.getElementById("<%=btnDerived.ClientID %>");
            obj.click();

            var json = {
                originBillNo: $('#AorderCode').val(),
                customerName: $('#AcustomerName').val(),
                consigneeName: $('#AconsigneeName').val(),
                InCreateStatus: $("#AInCreateStatus").combobox('getValue'),
                StartDate: $('#StartDate').datebox('getValue'),
                EndDate: $('#EndDate').datebox('getValue'),
            }
            $.ajax({
                url: "orderApi.aspx?method=QueryTMallOrderExport&key=" + JSON.stringify(json),
                success: function (text) {
                    if (text == "OK") { var obj = document.getElementById("<%=btnDerived.ClientID %>"); obj.click(); }
                    else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text, 'warning'); }
                }
            });
        }

