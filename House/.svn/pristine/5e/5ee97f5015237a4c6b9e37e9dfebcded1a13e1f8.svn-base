    <script type="text/javascript">
        //修改付款涵
        function modifyRemit(Did) {
            var row = $("#dgRemit").datagrid('getData').rows[Did];
            if (row) {
                $('#dvPrint').dialog('open').dialog('setTitle', '修改' + row.CardName + '的付款涵');
                $('#fmPrint').form('clear');
                $('#fmPrint').form('load', row);
                var datenow = new Date();
                $('#NowDate').datebox('setValue', getNowFormatDate(datenow));
                $('#PayMoneyDX').textbox('setValue', atoc(row.PayMoney));
                //收款人姓名
                $('#CardName').combobox({
                    valueField: 'CardName', textField: 'CardName', delay: '10',
                    url: '../Finance/financeApi.aspx?method=QueryReceiveNameList',
                    onSelect: onReceiveNameChanged,
                    required: true
                });
                $('#ApplyPeople').textbox('readonly', true);
                $('#RelateAwb').textbox('readonly', true);
                $('#PayMoney').numberbox('readonly', false);
                $('#PayMoneyDX').textbox('readonly', false);
                $('#btnPrint').linkbutton({ text: '&nbsp;保存成功后打印&nbsp;' });
                $('#CardName').combobox('setValue', row.CardName);
                bindMethod();
            }
        }

        //查询付款涵
        function RemitQuery() {
            var mOpts = $('#dgRemit').datagrid('options');
            mOpts.url = 'financeApi.aspx?method=RemitQuery';
            $('#dgRemit').datagrid('load', {
                ApplyPeople: $('#RApplyPeople').val(),
                RelateAwb: $('#RRelateAwb').val(),
                HouseID: $("#RHouseID").combobox('getValue'),
                ReceiveUnit: $('#RReceiveUnit').val(),
                CardName: $('#RCardName').val(),
                CardNum: $('#RCardNum').val(),
                StartDate: $('#RStartDate').datebox('getValue'),
                EndDate: $('#REndDate').datebox('getValue'),
                CardBank: $('#RCardBank').val()
            });
        }
        //打开汇款涵界面
        function Remit() {
            $('#dvRemit').dialog('open').dialog('setTitle', '汇款涵管理');
            var datenow = new Date();
            $('#RStartDate').datebox('setValue', getNowFormatDate(datenow));
            $('#REndDate').datebox('setValue', getNowFormatDate(datenow));
            //所在仓库
            $('#RHouseID').combobox({
                url: '../House/houseApi.aspx?method=CargoPermisionHouse',
                valueField: 'HouseID', textField: 'Name'
            });
            $('#dgRemit').datagrid({
                width: '100%',
                height: '450',
                title: '', //标题内容
                loadMsg: '数据加载中请稍候...',
                autoRowHeight: false, //行高是否自动
                collapsible: true, //是否可折叠
                pagination: true, //分页是否显示
                pageSize: 12, //每页多少条
                pageList: [12, 20],
                fitColumns: false, //设置为 true，则会自动扩大或缩小列的尺寸以适应网格的宽度并且防止水平滚动
                singleSelect: false, //设置为 true，则只允许选中一行。
                checkOnSelect: true, //如果设置为 true，当用户点击某一行时，则会选中/取消选中复选框。如果设置为 false 时，只有当用户点击了复选框时，才会选中/取消选中复选框
                idField: 'ID',
                url: null,
                toolbar: '#Remittoolbar',
                columns: [[
                    { title: '', field: '', checkbox: true, width: '30px' },
                    { title: '申请部门', field: 'ApplyDep', width: '80px' },
                    { title: '申请人', field: 'ApplyPeople', width: '70px' },
                    { title: '收款单位', field: 'ReceiveUnit', width: '80px' },
                    { title: '收款人', field: 'CardName', width: '80px' },
                    { title: '开户行', field: 'CardBank', width: '120px' },
                    { title: '收款账号', field: 'CardNum', width: '120px' },
                    { title: '付款金额', field: 'PayMoney', width: '80px' },
                    {
                        title: '付款方式', field: 'PayWay', width: '60px', formatter: function (val, row, index) {
                            if (val == "1") { return "对公"; } else if (val == "2") { return "对私"; } else if (val == "3") { return "油卡"; } else if (val == "4") { return "借支单核销"; } else { return "现金"; }
                        }
                    },
                    { title: '付款说明', field: 'PayMemo', width: '90px' },
                    { title: '关联单号', field: 'RelateAwb', width: '80px' },
                    { title: '所属仓库', field: 'HouseName', width: '70px' },
                    { title: '打印次数', field: 'PrintNum', width: '60px' },
                    { title: '操作时间', field: 'OP_DATE', width: '120px', formatter: DateTimeFormatter }
                ]],
                onLoadSuccess: function (data) { },
                onDblClickRow: function (index, row) { modifyRemit(index); }
            });
        }
        //打印付款申请书
        function PayPrint() {
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定打印付款申请书？', function (r) {
                if (r) {
                    $('#fmPrint').form('submit', {
                        url: 'financeApi.aspx?method=SavePayment',
                        contentType: "application/json;charset=utf-8", dataType: "json",
                        onSubmit: function (param) {
                            param.ACardNum = $('#CardNum').combobox('getText');
                            param.ACardName = $('#CardName').combobox('getText');
                            var trd = $(this).form('enableValidation').form('validate');
                            return trd;
                        },
                        success: function (msg) { }
                    });
                    //打印
                    DivPayPrint();
                }
            });
        }
        //绑定费用框
        function bindMethod() {
            $("#PayMoney").numberbox({ "onChange": function (newValue, oldValue) { $('#PayMoneyDX').textbox('setValue', atoc(Number(newValue))); } });
        }
        //显示打印付款申请书
        function showPrint() {
            bindMethod();
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要打印的数据！', 'warning'); return; }
            var datenow = new Date();
            $('#NowDate').datebox('setValue', getNowFormatDate(datenow));
            var contractN = "";
            var TFee = 0;
            var ApplyPeople = null;
            var ApplyDep = null;
            for (var i = 0, l = rows.length; i < l; i++) {
                TFee += Number(rows[i].ExCharge); //报销金额
                contractN += rows[i].ExID + ","; //报销单号
                if (ApplyPeople != null) {
                    if (ApplyPeople != rows[i].ExName) {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '只能打印同一报销人的汇款函！', 'warning');
                        return;
                    }
                } else {
                    ApplyPeople = rows[i].ExName;
                    ApplyDep = rows[i].ExDepart;
                }
            }
            $('#dvPrint').dialog('open').dialog('setTitle', '打印付款申请书');
            $('#ApplyDep').textbox('setValue', ApplyDep);
            $('#ApplyPeople').textbox('setValue', ApplyPeople);
            $('#ChargeUnit').textbox('setValue', rows[0].ChargeUnit);
            //收款人姓名
            $('#CardName').combobox({
                valueField: 'CardName', textField: 'CardName', delay: '10',
                url: '../Finance/financeApi.aspx?method=QueryReceiveNameList',
                onSelect: onReceiveNameChanged,
                required: true
            });
            $('#ApplyPeople').textbox('readonly', false);
            $('#RelateAwb').textbox('readonly', false);
            $('#PayMoney').numberbox('readonly', false);
            $('#PayMoneyDX').textbox('readonly', false);
            //$('#ReceiveUnit').textbox('setValue', '');
            $('#RelateAwb').textbox('setValue', contractN);
            //$('#CardName').textbox('setValue', '');
            $('#CardBank').textbox('setValue', '');
            $('#CardNum').textbox('setValue', '');
            $('#PayMoney').textbox('setValue', Number(TFee).toFixed(2));
            $('#PayMoneyDX').textbox('setValue', atoc(TFee));
            $('#PayMemo').val('');
            $('#btnPrint').linkbutton({ text: '&nbsp;打&nbsp;印&nbsp;' });

        }
        //收货人自动选择方法
        function onReceiveNameChanged(item) {
            if (item) {
                $('#ReceiveUnit').textbox('setValue', item.ReceiveUnit);
                var url = "../Finance/financeApi.aspx?method=QueryReceiveInfo&CardName=" + item.CardName;
                $('#CardNum').combobox('reload', url);
                $('#CardNum').combobox('setValue', '');
            }
        }
        function onCardNumChanged(item) {
            if (item) {
                $('#ReceiveUnit').textbox('setValue', item.ReceiveUnit);//ClientShortName
                $('#CardBank').textbox('setValue', item.CardBank);
                //$('input:checkbox[name="PayWay"][value="1"]').prop('checked', true); //打印运单
            }
        }
        //修改
        function editItem() {
            $('#IsSave').val("1");
            var row = $('#dg').datagrid('getSelected');
            if (row == null || row == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要修改的数据！', 'warning'); return; }
            if (row) {
                if (row.CheckStatus == "1") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '已结算的单不能修改！', 'warning'); return; }
                $('#dlgAwbDetail').dialog('open').dialog('setTitle', '修改报销');
                ShowDG();
                $('#dgAdd').datagrid('loadData', { total: 0, rows: [] });
                $('#fm').form('clear');
                $.ajax({
                    url: "../Finance/financeApi.aspx?method=GetExpenseById&id=" + row.ExID,
                    cache: false,
                    success: function (text) {
                        var o = eval('(' + text + ')');
                        o.ExpenseDate = AllDateTime(o.ExpenseDate);
                        $('#fm').form('load', o);
                        if (o.OneCheckID != undefined && o.OneCheckID != "") { info = "一级审批:" + o.OneCheckName; }
                        if (o.TwoCheckID != undefined && o.TwoCheckID != "") { info = "<br />二级审批:" + o.TwoCheckName; }
                        if (o.ThreeCheckID != undefined && o.ThreeCheckID != "") { info = "<br />三级审批:" + o.ThreeCheckName; }
                        if (o.FourCheckID != undefined && o.FourCheckID != "") { info = "<br />四级审批:" + o.FourCheckName; }
                        if (o.FiveCheckID != undefined && o.FiveCheckID != "") { info = "<br />五级审批:" + o.FiveCheckName; }
                        if (o.SixCheckID != undefined && o.SixCheckID != "") { info = "<br />六级审批:" + o.SixCheckName; }
                        //$('#ApproveInfo').html("一级审批：" + rec.SupervisorName + "<br />二级审批：" + rec.FinanceName + "<br />三级审批：" + rec.LeaderName);
                        $('#ApproveInfo').html(info);
                        for (var i = 0; i < o.exDetail.length; i++) {
                            var rows = { HappenDate: o.exDetail[i].HappenDate, Memo: o.exDetail[i].Memo, Summary: o.exDetail[i].Summary, DetailCharge: o.exDetail[i].DetailCharge, DetailID: o.exDetail[i].DetailID, ZID: o.exDetail[i].ZID, ZName: o.exDetail[i].ZName, FName: o.exDetail[i].FName, FID: o.exDetail[i].FID, SID: o.exDetail[i].SID, SName: o.exDetail[i].SName };
                            $('#dgAdd').datagrid('appendRow', rows);
                        }
                        if (o.Status == "0" || o.Status == "2") { $('#save').linkbutton('enable'); } else { $('#save').linkbutton('disable'); }
                    }
                });
                //报销部门
                $('#AExDepart').combobox({
                    valueField: 'Name', textField: 'Name', delay: '10',
                    url: '../Finance/financeApi.aspx?method=QuerySystemOrganize',
                    required: true
                });
            }
        }
