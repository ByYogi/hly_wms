        /// <summary>
        /// 库存导入Excel文件
        /// </summary>
        public void saveFile()
        {
            System.Web.HttpFileCollection files = this.Request.Files;
            if (files == null || files.Count == 0) return;
            string attachmentId = Guid.NewGuid().ToString();
            DataTable data = ToExcel.ImportExcelData(files);

            CargoImportEntity import = new CargoImportEntity();
            import.Result = true;
            import.Data = "";
            import.Message = "";
            import.Type = 0;
            import.ExistCount = 0;
            string msg = "";

            //List<CargoFactoryOrderEntity> ent = new List<CargoFactoryOrderEntity>();

            CargoFactoryOrderBus FacBus = new CargoFactoryOrderBus();

            //验证上传excel文件列数是否有效
            if (data.Columns.Count != 5)
            {
                import.Result = false;
                import.Type = 1;
                import.Message = "模板有误或缺少列，请重新下载模板";
                String abnormalJson = JSON.Encode(import);
                Response.Clear();
                Response.Write(abnormalJson);
                Response.End();
                return;
            }
            //清空table中的空行
            removeEmpty(data);


            if (data.Rows.Count <= 0)
            {
                import.Result = false;
                import.Type = 1;
                import.Message = "Excel无有效数据，请检查导入数据";
                String abnormalJson = JSON.Encode(import);
                Response.Clear();
                Response.Write(abnormalJson);
                Response.End();
                return;
            }


            //获取所有采购供应商信息
            CargoClientBus clientBus = new CargoClientBus();
            //获取所有可用品牌用于表格显示品牌
            CargoProductBasicPriceBus spescBus = new CargoProductBasicPriceBus();

            //获取所有仓库用于表格显示仓库
            List<CargoProductBasicPriceEntity> allHouse = new List<CargoProductBasicPriceEntity>();
            Dictionary<string, int> dicHouse = new Dictionary<string, int>();
            foreach (var item in UserInfor.CargoList)
            {
                if (!dicHouse.ContainsKey(item.Name))
                {
                    dicHouse.Add(item.Name, item.ID);
                }
            }

            CargoHouseBus houseBus = new CargoHouseBus();
            var AreaList = houseBus.GetHouseAreaList();

            //获取产品规格
            DataTable ProductBasicDt = new DataTable();
            ProductBasicDt = spescBus.QueryProductSpecDate(new CargoProductEntity { });

            DataTable newData = new DataTable();
            newData.Columns.Add("HouseID");
            newData.Columns.Add("HouseName");
            newData.Columns.Add("AreaID");
            newData.Columns.Add("AreaName");
            newData.Columns.Add("TypeID");
            newData.Columns.Add("TypeName");
            newData.Columns.Add("GoodsCode");
            newData.Columns.Add("Specs");
            newData.Columns.Add("Figure");
            newData.Columns.Add("LoadIndex");
            newData.Columns.Add("SpeedLevel");
            newData.Columns.Add("StockNum");
            newData.Columns.Add("MinStock");
            newData.Columns.Add("MaxStock");
            newData.Columns.Add("ProductCode");
            newData.Columns.Add("ProductName");
            newData.Columns.Add("Model");

            int abnormalCount = 0;
            for (int i = 0; i < data.Rows.Count; i++)
            {
                //验证Excel表格指定列是否缺少数据
                bool isContinue = false;
                for (int j = 0; j < data.Columns.Count; j++)
                {
                    if (string.IsNullOrEmpty((data.Rows[i][j]).ToString()))
                    {
                        isContinue = true;
                        break;
                    }
                }
                if (isContinue)
                {
                    abnormalCount++;
                    msg += "第" + (i + 2) + "行数据有空值\r\n";
                    continue;
                }
                //验证导入仓库是否在数据库中有效，无效则跳过
                int HouseID = 0;
                if (!string.IsNullOrEmpty(Convert.ToString(data.Rows[i][0]).Trim()))
                {
                    if (!dicHouse.ContainsKey(Convert.ToString(data.Rows[i][0]).Trim()))
                    {
                        abnormalCount++;
                        msg += "第" + (i + 2) + "行所属仓库不存在\r\n";
                        continue;
                    }
                    else
                    {
                        dicHouse.TryGetValue(Convert.ToString(data.Rows[i][0]).Trim(), out HouseID);
                    }
                }

                //验证最小库存是否为数字
                if (!string.IsNullOrEmpty(data.Rows[i][2].ToString().Trim()))
                {
                    if (!IsNumber(data.Rows[i][2].ToString().Trim()))
                    {
                        abnormalCount++;
                        msg += "第" + (i + 1) + "行数据最小库存数量有误\r\n";
                        continue;
                    }
                    data.Rows[i][2] = data.Rows[i][2].ToString().Trim();
                }
                else
                {
                    data.Rows[i][2] = 0;
                }
                //验证最大库存是否为数字
                if (!string.IsNullOrEmpty(data.Rows[i][3].ToString().Trim()))
                {
                    if (!IsNumber(data.Rows[i][3].ToString().Trim()))
                    {
                        abnormalCount++;
                        msg += "第" + (i + 1) + "行数据最大库存数量有误\r\n";
                        continue;
                    }
                    data.Rows[i][3] = data.Rows[i][3].ToString().Trim();
                }
                else
                {
                    data.Rows[i][3] = 0;
                }
                //验证安全库存是否为数字
                if (!string.IsNullOrEmpty(data.Rows[i][4].ToString().Trim()))
                {
                    if (!IsNumber(data.Rows[i][4].ToString().Trim()))
                    {
                        abnormalCount++;
                        msg += "第" + (i + 1) + "行数据安全库存数量有误\r\n";
                        continue;
                    }
                    data.Rows[i][4] = data.Rows[i][4].ToString().Trim();
                }
                else
                {
                    data.Rows[i][4] = 0;
                }

                int StockNum = Convert.ToInt32(Convert.ToString(data.Rows[i][4]).Trim());
                int MaxStock = Convert.ToInt32(Convert.ToString(data.Rows[i][3]).Trim());
                int MinStock = Convert.ToInt32(Convert.ToString(data.Rows[i][2]).Trim());
                string ProductCode = Convert.ToString(data.Rows[i][1]).Trim();
                string ProductName = string.Empty;
                string Model = string.Empty;
                string Specs = string.Empty;
                string Figure = string.Empty;
                string LoadIndex = string.Empty;
                string SpeedLevel = string.Empty;
                string GoodsCode = string.Empty;
                string TypeID = string.Empty;
                string TypeName = string.Empty;
                var Area = AreaList.FirstOrDefault(a => a.HouseID == HouseID);
                var AreaID = Area.AreaID;
                var AreaName = Area.Name;

                DataRow[] rows = ProductBasicDt.Select("ProductCode='" + ProductCode + "'");
                if (rows.Count() <= 0)
                {
                    abnormalCount++;
                    msg += "第" + (i + 2) + "行数据基础规格不存在\r\n";
                    continue;
                }
                else
                {
                    DataRow row = rows[0];
                    ProductName = string.IsNullOrEmpty(Convert.ToString(row[16]).Trim()) ? "" : Convert.ToString(row[16]).Trim();
                    TypeID = Convert.ToString(row[1]).Trim();
                    TypeName = Convert.ToString(row[33]).Trim();
                    GoodsCode = Convert.ToString(row[3]).Trim();
                    Specs = Convert.ToString(row[4]).Trim();
                    Figure = Convert.ToString(row[5]).Trim();
                    LoadIndex = Convert.ToString(row[9]).Trim();
                    SpeedLevel = Convert.ToString(row[10]).Trim().Length > 1 ? Convert.ToString(row[10]).Trim().Substring(0, 1) : Convert.ToString(row[10]).Trim();
                    Model = Convert.ToString(row[2]).Trim();
                }


                DataRow[] NewRows = newData.Select("ProductCode='" + ProductCode + "'");
                DataRow drNew = newData.NewRow();

                object[] objs = {
                            HouseID,
                            Convert.ToString(data.Rows[i][0]).Trim(),
                            AreaID,
                            AreaName,
                            TypeID,
                            TypeName,
                            GoodsCode,
                            Specs,
                            Figure,
                            LoadIndex,
                            SpeedLevel,
                            StockNum,
                            MinStock,
                            MaxStock,
                            ProductCode,
                            ProductName,
                            Model,
                    };
                drNew.ItemArray = objs;
                newData.Rows.Add(drNew);

            }
            //赋值表格
            List<CargoSafeStockEntity> entityFiles = new List<CargoSafeStockEntity>();
            if (newData.Rows.Count > 0)
            {
                for (int i = 0; i < newData.Rows.Count; i++)
                {
                    CargoSafeStockEntity ent = new CargoSafeStockEntity();
                    ent.ExcelNo = i + 1;
                    ent.HouseID = Convert.ToString(newData.Rows[i][0]);
                    ent.HouseName = Convert.ToString(newData.Rows[i][1]).Trim();
