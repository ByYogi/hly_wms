    <script type="text/javascript">

        //导出数据
        function AwbExport(ty) {
            var row = $("#dg").datagrid('getData').rows[0];
            if (row == null) { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '没有数据可以进行导出，请重新查询！', 'warning'); return; }
            var lm = "<%=UserInfor.LoginName%>";
            var sm = $('#SaleManID').combobox('getValue');
            if (sm == undefined) { sm = ""; }
            if (lm == "2132") {
                if (sm == undefined || sm == "") {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择业务员！', 'warning');
                    return;
                }
                if (sm != "2114" && sm != "2113" && sm != "2112" && sm != "2111" && sm != "2110") {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择贵公司业务员！', 'warning');
                    return;
                }
            }
            var roleName = "<%=UserInfor.RoleCName%>";
            var ThrowGood = $("#AThrowGood").combobox('getValue');
            //if (lm == "2288" && ThrowGood == -1) {
            //    ThrowGood = "0,1,4,8,13,15,16,17,18,20";
            //}
            var RoleID = "<%=UserInfor.RoleID%>";
            if (RoleID == "42" && ThrowGood == -1) {
                ThrowGood = "0,4,8,16,17,20";

            }
            if (RoleCName.indexOf("锦湖") >= 0) {
                ThrowGood = '21,5';
            }
            $.messager.progress({ msg: '请稍后,正在导出中...' });
            $.ajax({
                //url: "reportApi.aspx?method=QuerySaleManDailyReportForExport&key=" + escape(key.toString()),
                url: "reportApi.aspx?method=QuerySaleManDailyReportForExport&StartDate=" + $('#StartDate').datebox('getValue') + "&EndDate=" + $('#EndDate').datebox('getValue') + "&SaleManID=" + sm + "&HouseID=" + $("#AHouseID").combobox('getValues').toString() + "&AreaID=" + $("#AreaID").combobox('getValue') + "&Specs=" + encodeURIComponent($('#ASpecs').val()) + "&Figure=" + encodeURIComponent($('#AFigure').val()) + "&ClientName=" + encodeURIComponent($('#AClientName').datebox('getValue')) + "&AcceptPeople=" + encodeURIComponent($('#AAcceptPeople').val()) + "&PID=" + $("#APID").combobox('getValue') + "&SID=" + $("#ASID").combobox('getValue') + "&OrderType=" + $("#AOrderType").combobox('getValue') + "&ThrowGood=" + ThrowGood + "&BelongHouse=0" + "&ProductID=" + $('#AProductID').val() + '&BusinessID=' + $('#BusinessID').combobox('getValue') + '&IsPriceAlert=' + $('#IsPriceAlert').val(),//所属部门,
                success: function (text) {
                    $.messager.progress("close");
                    if (text == "OK") {
                        if (ty == "0") {
                            var obj = document.getElementById("<%=btnDerived.ClientID %>"); obj.click();
                        }
                        else {
                            var obj = document.getElementById("<%=btnAwbReconExport.ClientID %>"); obj.click();
                        }
                    }
                    else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text, 'warning'); }
                }
            });
        }

    </script>

