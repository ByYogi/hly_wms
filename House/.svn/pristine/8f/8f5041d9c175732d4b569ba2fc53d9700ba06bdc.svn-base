    <script src="../JS/Lodop/LodopFuncs.js" type="text/javascript"></script>
    <script type="text/javascript">

        //删除信息
        function DelItem() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要删除的数据！', 'warning');
                return;
            }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定删除？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: 'orderApi.aspx?method=DelArrivalOrderData',
                        type: 'post', dataType: 'json', data: { data: json },
                        success: function (text) {
                            //var result = eval('(' + msg + ')');
                            if (text.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '删除成功!', 'info');
                                dosearch();
                            }
                            else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        //导出数据
        function AwbExport() {
            var row = $("#dg").datagrid('getData').rows[0];
            if (row == null) { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '没有数据可以进行导出，请重新查询！', 'warning'); return; }
            $.messager.progress({ msg: '请稍后,正在导出中...' });
            $.ajax({
                url: "orderApi.aspx?method=QueryPurchaseOrderInfoExport&OrderID=" + $('#AOrderID').val() + "&AcceptUnit=" + $('#AAcceptUnit').val() + "&StartDate=" + $('#StartDate').datebox('getValue') + "&EndDate=" + $('#EndDate').datebox('getValue') + "&HouseID=" + $("#AHouseID").combobox('getValue'),
                success: function (text) {
                    $.messager.progress("close");
                    if (text == "OK") { var obj = document.getElementById("<%=btnDerived.ClientID %>"); obj.click() }
                    else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text, 'warning'); }
                }
            });
            $.messager.progress("close");
        }

        //双击显示订单详细界面 
        function editItemByID(Did) {
            $('#outDg').datagrid('loadData', { total: 0, rows: [] });
            var row = $("#dg").datagrid('getData').rows[Did];
            if (row) {
                $('#dlgOrder').dialog('open').dialog('setTitle', '查看进货单');
                $('#fmDep').form('clear');
                $('#fmDep').form('load', row);
                showGrid();
                var gridOpts = $('#outDg').datagrid('options');
                gridOpts.url = 'orderApi.aspx?method=QueryPurchaseOrderByOrderID&OrderID=' + row.OrderID;
            }
        }
        var editIndex = undefined;
        function endEditing() {
            if (editIndex == undefined) { return true }
            if ($('#outDg').datagrid('validateRow', editIndex)) {
                var ed = $('#outDg').datagrid('getEditor', { index: editIndex });

                //var productname = $(ed.target).combobox('getText');
                //$('#outDg').datagrid('getRows')[editIndex]['productname'] = productname;
                $('#outDg').datagrid('endEdit', editIndex);
                editIndex = undefined;
                return true;
            } else {
                return false;
            }
        }
        function onClickRow(index) {
            var rows = $("#outDg").datagrid('getData').rows[index];
            if (rows.InCargoStatus == "0") {
                if (editIndex != index) {
                    if (endEditing()) {
                        $('#outDg').datagrid('selectRow', index)
                            .datagrid('beginEdit', index);
                        editIndex = index;
                    } else {
                        $('#outDg').datagrid('selectRow', editIndex);
                    }
                }
            } else {
                $('#outDg').datagrid('endEdit', editIndex);
                editIndex = undefined;
                $('#outDg').datagrid('acceptChanges');
                var TPiece = 0; var TCharge = 0;
                var rows = $('#outDg').datagrid('getRows');
                for (var i = 0; i < rows.length; i++) {
                    TPiece += Number(rows[i].InPiece);
                    TCharge += Number(accMul(rows[i].InPiece, rows[i].UnitPrice));
                }

                $('#TransportFee').numberbox('setValue', TCharge);
                $('#TotalCharge').numberbox('setValue', TCharge);
                $('#Piece').numberbox('setValue', TPiece);
            }

        }
        function accMul(arg1, arg2) {
            var m = 0, s1 = arg1.toString(), s2 = arg2.toString();
            try { m += s1.split(".")[1].length } catch (e) { }
            try { m += s2.split(".")[1].length } catch (e) { }
            return Number(s1.replace(".", "")) * Number(s2.replace(".", "")) / Math.pow(10, m)
        }
        //显示列表
        function showGrid() {
            var columns = [];
            //columns.push({ title: '', field: '', checkbox: true, width: '30px' });
            columns.push({ title: '品牌', field: 'TypeName', width: '60px' });
           
            columns.push({
                title: '入库状态', field: 'InCargoStatus', width: '70px', formatter: function (val, row, index) {
                    if (val == "0") { return "<span title='未入库'>未入库</span>"; }
                    else if (val == "1") { return "<span title='已入库'>已入库</span>"; }
                    else if (val == "2") { return "<span title='部分入库'>部分入库</span>" }
                    else { return "<span title='未知'>未知</span>" }
                }
            });
            columns.push({ title: '规格', field: 'Specs', width: '80px' });
            columns.push({ title: '花纹', field: 'Figure', width: '80px' });
            columns.push({
                title: '载速', field: 'LoadIndex', width: '60px', formatter: function (value, row) {
                    return "<span title='" + value + row.SpeedLevel + "'>" + value + row.SpeedLevel + "</span>";
                }
            });
            //columns.push({ title: '速度', field: 'SpeedLevel', width: '50px' });
            columns.push({ title: '货品代码', field: 'GoodsCode', width: '80px' });
            columns.push({ title: '批次', field: 'Batch', width: '50px' });
            columns.push({ title: '入库数量', field: 'Numbers', width: '60px', align: 'right' });
            columns.push({ title: '进货数量', field: 'InPiece', width: '60px', align: 'right', editor: { type: 'numberbox' } });
            columns.push({ title: '进货价', field: 'UnitPrice', width: '60px', align: 'right', editor: { type: 'numberbox', options: { precision: 2 } } });
            columns.push({ title: '成本价', field: 'CostPrice', width: '60px', align: 'right', editor: { type: 'numberbox', options: { precision: 2 } } });
            columns.push({ title: '门店价', field: 'TradePrice', width: '60px', align: 'right', editor: { type: 'numberbox', options: { precision: 2 } } });
            columns.push({ title: '销售价', field: 'SalePrice', width: '60px', align: 'right', editor: { type: 'numberbox', options: { precision: 2 } } });
            //columns.push({ title: '型号', field: 'Model', width: '55px' });
            $('#outDg').datagrid({
                width: '100%',
                height: '350px',
                title: '', //标题内容
                rownumbers: true,
                loadMsg: '数据加载中请稍候...',
                autoRowHeight: false, //行高是否自动
                collapsible: false, //是否可折叠
                pagination: false, //分页是否显示
                fitColumns: false, //设置为 true，则会自动扩大或缩小列的尺寸以适应网格的宽度并且防止水平滚动
                singleSelect: true, //设置为 true，则只允许选中一行。
                checkOnSelect: true, //如果设置为 true，当用户点击某一行时，则会选中/取消选中复选框。如果设置为 false 时，只有当用户点击了复选框时，才会选中/取消选中复选框
                idField: 'ProductID',
                url: null,
                toolbar: '#toolbar',
                columns: [columns],
                rowStyler: function (index, row) {
                    if (row.InCargoStatus == "0" || row.InCargoStatus == "2") { return "background-color:#f9e7e6"; }
                },
                onClickRow: onClickRow
            });
        }
        function save() {
            $('#outDg').datagrid('acceptChanges')
            //取消业务员禁用方便后台取值
            var rows = $('#outDg').datagrid('getRows');
            if (rows == null || rows == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要保存的数据！', 'warning'); return; }

            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定保存？', function (r) {
                if (r) {
                    //启用复选框用于后台数据获取
                    $.messager.progress({ msg: '请稍后,正在保存中...' });
                    $('#btnSave').linkbutton('disable');
                    var json = JSON.stringify(rows);
                    $('#fmDep').form('submit', {
                        url: 'orderApi.aspx?method=UpdateArrivalOrderData',
                        contentType: "application/json;charset=utf-8", dataType: "json",
                        onSubmit: function (param) {
                            param.AAcceptUnit = $('#AcceptUnit').combobox('getText');
                            param.submitData = json;
                            var trd = $(this).form('enableValidation').form('validate');
                            if (trd) { $('#btnSave').linkbutton('disable'); } else {
