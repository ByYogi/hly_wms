<%@ Page Title="仓库区域管理" Language="C#" MasterPageFile="~/Site.Master" AutoEventWireup="true" CodeBehind="houseAreaManager.aspx.cs" Inherits="Cargo.House.houseAreaManager" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
    <script type="text/javascript">
        //页面加载显示遮罩层
        var pc;
        $.parser.onComplete = function () {
            if (pc) {
                clearTimeout(pc);
            }
            pc = setTimeout(closemask, 10);
        }
        //关闭加载中遮罩层
        function closemask() {
            $("#Loading").fadeOut("normal", function () {
                $(this).remove();
            });
        }
        window.onload = function () {
            adjustment();

            //如果是广州、广东、揭阳的用户开放区域大仓选项
            if ("<%=UserInfor.IsAdmin%>" == "1") {
                $('#UpdTypeItem').show();
            } else {
                $('#UpdTypeItem').hide();
            }
        }
        $(window).resize(function () {
            adjustment();
        });
        function adjustment() {
            var height = Number($(window).height()) - $("div[name='SelectDiv1']").outerHeight(true);
            $('#dg').datagrid({ height: height });
        }
        $(document).ready(function () {
            $('#dg').datagrid({
                width: '100%',
                title: '', //标题内容
                loadMsg: '数据加载中请稍候...',
                autoRowHeight: false, //行高是否自动
                collapsible: true, //是否可折叠
                pagination: true, //分页是否显示
                pageSize: Math.floor((Number($(window).height()) - $("div[name='SelectDiv1']").outerHeight(true) - 58) / 28), //每页多少条
                pageList: [Math.floor((Number($(window).height()) - $("div[name='SelectDiv1']").outerHeight(true) - 58) / 28), Math.floor((Number($(window).height()) - $("div[name='SelectDiv1']").outerHeight(true) - 58) / 28) * 2],
                fitColumns: false, //设置为 true，则会自动扩大或缩小列的尺寸以适应网格的宽度并且防止水平滚动
                singleSelect: false, //设置为 true，则只允许选中一行。
                checkOnSelect: true, //如果设置为 true，当用户点击某一行时，则会选中/取消选中复选框。如果设置为 false 时，只有当用户点击了复选框时，才会选中/取消选中复选框
                idField: 'AreaID',
                url: null,
                toolbar: '#toolbar',
                columns: [[
                    { title: '', field: 'AreaID', checkbox: true, width: '2%' },
                    {
                        title: '区域名称', field: 'Name', width: '10%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '区域代码', field: 'Code', width: '4%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '订单代码', field: 'OrderCode', width: '4%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '出发城市', field: 'OrderDep', width: '4%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '上级区域', field: 'ParentName', width: '10%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '上级仓库', field: 'MainHouseName', width: '10%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '所属仓库', field: 'HouseName', width: '8%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '承运物流', field: 'LogisticName', width: '8%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '配送区域', field: 'DeliveryArea', width: '20%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    {
                        title: '备注', field: 'Remark', width: '20%', formatter: function (value) {
                            return "<span title='" + value + "'>" + value + "</span>";
                        }
                    },
                    { title: '操作时间', field: 'OP_DATE', width: '10%', formatter: DateTimeFormatter }
                ]],
                onLoadSuccess: function (data) { },
                onDblClickRow: function (index, row) { editItemByID(index); },
                rowStyler: function (index, row) {
                    if (row.DelFlag == "1") { return "background-color:#D2D2D2"; }
                }
            });

            //所在仓库
            $('#HID').combobox({
                url: 'houseApi.aspx?method=CargoPermisionHouse',
                valueField: 'HouseID', textField: 'Name',
                onSelect: function (rec) {
                    $('#PID').combobox('clear');
                    var url = 'houseApi.aspx?method=QueryALLArea&pid=0&hid=' + rec.HouseID;
                    $('#PID').combobox('reload', url);
                    //一级仓库
                    $('#PID').combobox({
                        onSelect: function (fai) {
                            $('#SID').combobox('clear');
                            var url = 'houseApi.aspx?method=QueryALLArea&hid=' + rec.HouseID + '&pid=' + fai.AreaID;
                            $('#SID').combobox('reload', url);
                            $('#SID').combobox('textbox').bind('focus', function () { $('#SID').combobox('showPanel'); });
                        },
                        onChange: function (newValue, oldValue) {
                            if (newValue != undefined) {

                            }
                        }
                    });
                    $('#PID').combobox('textbox').bind('focus', function () { $('#PID').combobox('showPanel'); });
                }
            });

            //所在仓库
            $('#HouseID').combobox({
                url: 'houseApi.aspx?method=CargoPermisionHouse',
                valueField: 'HouseID', textField: 'Name',
                onSelect: function (rec) {
                    $('#ParentID').combobox('clear');
                    var url = 'houseApi.aspx?method=QueryALLArea&pid=0&hid=' + rec.HouseID;
                    $('#ParentID').combobox('reload', url);

                    //$("#Address").textbox('options').required = true;
                    //$("#Address").textbox('textbox').validatebox('options').required = true;
                    //$("#Address").textbox('validate');
                    $("#OrderCodeTr").show();
                    $("#LogisTr").show();
                    
                    $("#OrderDepTr").show();
                    //$("#AOrderCode").textbox('options').required = true;
                    //$("#AOrderCode").textbox('textbox').validatebox('options').required = true;
                    //$("#AOrderCode").textbox('validate');
                    $("#AOrderDep").textbox('options').required = true;
                    $("#AOrderDep").textbox('textbox').validatebox('options').required = true;
                    $("#AOrderDep").textbox('validate');
                }
            });
            //一级仓库
            $('#ParentID').combobox({
                onSelect: function (fai) {
                    $("#OrderCodeTr").hide();
                    $("#LogisTr").hide();
                    $("#OrderDepTr").hide();
                    $("#FirstAreaIDTr").show();
                    $("#AOrderCode").textbox('setValue', '');
                    $("#AOrderDep").textbox('setValue', '');
                    //$("#AOrderCode").textbox('options').required = false;
                    //$("#AOrderCode").textbox('textbox').validatebox('options').required = false;
                    //$("#AOrderCode").textbox('validate');
                    $("#AOrderDep").textbox('options').required = false;
                    $("#AOrderDep").textbox('textbox').validatebox('options').required = false;
                    $("#AOrderDep").textbox('validate');
                    $('#FirstAreaID').combobox('clear');
                    var url = 'houseApi.aspx?method=QueryALLArea&hid=' + $("#HouseID").combobox('getValue') + '&pid=' + fai.AreaID;
                    $('#FirstAreaID').combobox('reload', url);
                },
                onChange: function (newValue, oldValue) {
                    if (newValue == undefined || newValue == "") {
                        $("#OrderCodeTr").show();
                        $("#LogisTr").show();
                        $("#OrderDepTr").show();
                        $("#FirstAreaIDTr").hide();
                        $('#FirstAreaID').combobox('clear');
                        //$("#AOrderCode").textbox('options').required = true;
                        //$("#AOrderCode").textbox('textbox').validatebox('options').required = true;
                        //$("#AOrderCode").textbox('validate');
                        $("#AOrderDep").textbox('options').required = true;
                        $("#AOrderDep").textbox('textbox').validatebox('options').required = true;
                        $("#AOrderDep").textbox('validate');
                        //    $("#Address").textbox('options').required = true;
                        //    $("#Address").textbox('textbox').validatebox('options').required = true;
                        //    $("#Address").textbox('validate');
                    } else {
                        //    $("#Address").textbox('options').required = false;
                        //    $("#Address").textbox('textbox').validatebox('options').required = false;
                        //    $("#Address").textbox('validate');
                    }
                }
            });

            $('#HID').combobox('textbox').bind('focus', function () { $('#HID').combobox('showPanel'); });
            $('#ParentID').combobox('textbox').bind('focus', function () { $('#ParentID').combobox('showPanel'); });
            $('#FirstAreaID').combobox('textbox').bind('focus', function () { $('#FirstAreaID').combobox('showPanel'); });
            $('#HouseID').combobox('textbox').bind('focus', function () { $('#HouseID').combobox('showPanel'); });
        });
        //查询
        function dosearch() {
            $('#dg').datagrid('clearSelections');
            var gridOpts = $('#dg').datagrid('options');
            gridOpts.url = 'houseApi.aspx?method=QueryCargoArea';
            $('#dg').datagrid('load', {
                Name: $('#Name').val(),
                Code: $("#Code").val(),
                PID: $("#PID").combobox('getValue'),
                HID: $("#HID").combobox('getValue'),
                SID: $("#SID").combobox('getValue')
            });
        }
    </script>
</asp:Content>

