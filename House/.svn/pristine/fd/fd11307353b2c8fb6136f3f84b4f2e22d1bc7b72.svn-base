    <div id="dlg-buttons">
        <a href="#" class="easyui-linkbutton" iconcls="icon-ok" onclick="saveItem()">&nbsp;保&nbsp;存&nbsp;</a>&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="#" class="easyui-linkbutton" iconcls="icon-cancel" onclick="javascript:$('#dlg').dialog('close')">&nbsp;取&nbsp;消&nbsp;</a>
    </div>

    <script type="text/javascript">
        //新增区域信息
        function addItem() {
            $('#dlg').dialog('open').dialog('setTitle', '新增区域信息');
            $('#fm').form('clear');
            $('#ACode').textbox('readonly', false);
            $('#IsShowStock').combobox('setValue', 0);
            
            $("#Address").textbox('options').required = false;
            $("#Address").textbox('textbox').validatebox('options').required = false;
            $("#Address").textbox('validate');
            $("#OrderCodeTr").hide();
            $("#LogisTr").hide();
            
            $("#OrderDepTr").hide();
            $("#FirstAreaIDTr").hide();
        }
        //修改区域信息
        function editItem() {
            var row = $('#dg').datagrid('getSelected');
            if (row == null || row == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要修改的数据！', 'warning');
                return;
            }
            if (row) {
                $('#fm').form('clear');
                $('#dlg').dialog('open').dialog('setTitle', '修改区域信息');

                $('#ParentID').combobox('clear');
                var url = 'houseApi.aspx?method=QueryALLArea&pid=0&hid=' + row.HouseID;
                $('#ParentID').combobox('reload', url);

                if (row.MainHouseID != 0) {
                    $('#FirstAreaID').combobox('clear');
                    var url = 'houseApi.aspx?method=QueryALLArea&hid=' + row.HouseID + '&pid=' + row.MainHouseID;
                    $('#FirstAreaID').combobox('reload', url);
                }
                $('#fm').form('load', row);
                if (row.MainHouseID != 0) {
                    $('#ParentID').combobox('setValue', row.MainHouseID);
                    $('#FirstAreaID').combobox('setValue', row.ParentID);
                } else {
                    $('#ParentID').combobox('setValue', row.ParentID);
                }
                $('#ACode').textbox('readonly', true);
                if (row.ParentID == 0) {
                    $("#OrderCodeTr").show();
                    $("#LogisTr").show();
                    $("#OrderDepTr").show();
                    //$("#AOrderCode").textbox('options').required = true;
                    //$("#AOrderCode").textbox('textbox').validatebox('options').required = true;
                    //$("#AOrderCode").textbox('validate');
                    $("#AOrderDep").textbox('options').required = true;
                    $("#AOrderDep").textbox('textbox').validatebox('options').required = true;
                    $("#AOrderDep").textbox('validate');
                    //    $("#Address").textbox('options').required = true;
                    //    $("#Address").textbox('textbox').validatebox('options').required = true;
                    //    $("#Address").textbox('validate');
                } else {
                    $("#OrderCodeTr").hide();
                    $("#LogisTr").hide();
                    $("#OrderDepTr").hide();
                    //$("#AOrderCode").textbox('options').required = false;
                    //$("#AOrderCode").textbox('textbox').validatebox('options').required = false;
                    //$("#AOrderCode").textbox('validate');
                    $("#AOrderDep").textbox('options').required = false;
                    $("#AOrderDep").textbox('textbox').validatebox('options').required = false;
                    $("#AOrderDep").textbox('validate');
                    //    $("#Address").textbox('options').required = false;
                    //    $("#Address").textbox('textbox').validatebox('options').required = false;
                    //    $("#Address").textbox('validate');
                }
            }
        }
        //修改区域信息
        function editItemByID(Did) {
            var row = $("#dg").datagrid('getData').rows[Did];
            if (row) {
                $('#fm').form('clear');
                $('#dlg').dialog('open').dialog('setTitle', '修改区域信息');

                $('#ParentID').combobox('clear');
                var url = 'houseApi.aspx?method=QueryALLArea&pid=0&hid=' + row.HouseID;
                $('#ParentID').combobox('reload', url);

                if (row.MainHouseID != 0) {
                    $('#FirstAreaID').combobox('clear');
                    var url = 'houseApi.aspx?method=QueryALLArea&hid=' + row.HouseID + '&pid=' + row.MainHouseID;
                    $('#FirstAreaID').combobox('reload', url);
                }
                $('#fm').form('load', row);
                if (row.MainHouseID != 0) {
                    $('#ParentID').combobox('setValue', row.MainHouseID);
                    $('#FirstAreaID').combobox('setValue', row.ParentID);
                } else {
                    $('#ParentID').combobox('setValue', row.ParentID);
                }
                $('#ACode').textbox('readonly', true);

                if (row.ParentID == 0) {
                    $("#OrderCodeTr").show();
                    $("#LogisTr").show();
                    $("#OrderDepTr").show();
                    //$("#AOrderCode").textbox('options').required = true;
                    //$("#AOrderCode").textbox('textbox').validatebox('options').required = true;
                    //$("#AOrderCode").textbox('validate');
                    $("#AOrderDep").textbox('options').required = true;
                    $("#AOrderDep").textbox('textbox').validatebox('options').required = true;
                    $("#AOrderDep").textbox('validate');
                    //    $("#Address").textbox('options').required = true;
                    //    $("#Address").textbox('textbox').validatebox('options').required = true;
                    //    $("#Address").textbox('validate');
                } else {
                    $("#OrderCodeTr").hide();
                    $("#LogisTr").hide();
                    $("#OrderDepTr").hide();
                    //$("#AOrderCode").textbox('options').required = false;
                    //$("#AOrderCode").textbox('textbox').validatebox('options').required = false;
                    //$("#AOrderCode").textbox('validate');
                    $("#AOrderDep").textbox('options').required = false;
                    $("#AOrderDep").textbox('textbox').validatebox('options').required = false;
                    $("#AOrderDep").textbox('validate');
                    //    $("#Address").textbox('options').required = false;
                    //    $("#Address").textbox('textbox').validatebox('options').required = false;
                    //    $("#Address").textbox('validate');
                }
            }
        }
        //删除区域信息
        function DelItem() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要删除的数据！', 'warning');
                return;
            }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定删除？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: 'houseApi.aspx?method=DelCargoArea',
                        type: 'post', dataType: 'json', data: { data: json },
                        success: function (text) {
                            //var result = eval('(' + msg + ')');
                            if (text.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '删除成功!', 'info');
                                $('#dg').datagrid('reload');
                            }
                            else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        function UpdTypeItem() {
            var rows = $('#dg').datagrid('getSelections');
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要修改的数据！', 'warning');
                return;
            }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定修改？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows)
                    $.ajax({
                        url: 'houseApi.aspx?method=UpdateCargoAreaType',
                        type: 'post', dataType: 'json', data: { data: json },
                        success: function (text) {
                            //var result = eval('(' + msg + ')');
                            if (text.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '修改成功!', 'info');
                                $('#dg').datagrid('reload');
                            }
                            else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }

        //保存区域信息
        function saveItem() {
            $('#fm').form('submit', {
                url: 'houseApi.aspx?method=SaveCargoArea',
                onSubmit: function (param) {
                    param.LogisticName = $('#LogisID').combobox('getText');
                    param.VirtualInventory = "0";
                    var trd = $(this).form('enableValidation').form('validate');
                    return trd;
                    //return $(this).form('enableValidation').form('validate');
                },
                success: function (msg) {
                    var result = eval('(' + msg + ')');
                    if (result.Result) {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存成功!', 'info');
                        $('#dlg').dialog('close'); 	// close the dialog
                        dosearch();
                    } else {
                        $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存失败：' + result.Message, 'warning');
                    }
                }
            })
        }
    </script>

</asp:Content>
