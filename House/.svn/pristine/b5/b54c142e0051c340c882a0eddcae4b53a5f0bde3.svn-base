    <script type="text/javascript">
        //新增退货信息
        function addReturn() {
            var row = $('#dg').datagrid('getSelected');
            if (row == null || row == "" || row == undefined) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要生成退货单的数据！', 'warning');
                return;
            }
            else if (row.InCargoStatus != "" && (row.FacOrderNo != null || row.FacOrderNo != "" || row.FacOrderNo != undefined)) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', "该订单已生成退货单！", 'warning');
                return;
            }
            else if ((row.PayStatus != "2" || row.RefundCheckStatus != "0") || (row.InCargoStatus != "" && (row.FacOrderNo != null || row.FacOrderNo != "" || row.FacOrderNo != undefined))) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请仔细检查审核状态再生成退货单！', 'warning');
                return;
            } else if (row.PayStatus == "2" && row.RefundCheckStatus == "0" && row.OrderStatus == "0") {//未审核客户下单申请退款
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请仔细检查订单状态再生成退货单！', 'warning');
                return;
            } else {
                $('#dlg').dialog('open').dialog('setTitle', '生成退货单');
                $('#fm').form('clear');
                $('#WXPayOrderNo').textbox('setValue', row.WXPayOrderNo);
                $('#OrderNum').textbox('setValue', row.Piece);
                $('#WXPayOrderNo').textbox('disable');
                $('#OrderNum').textbox('disable');
                //$('#ReturnNum').numberbox('setValue', row.Piece);
                //$('#ReturnNum').numberbox('enable');
                $("#ATruckFlag1").prop('checked', true);
                $('#BHouseID').combobox({
                    url: '../House/houseApi.aspx?method=CargoPermisionHouse&HouseIds=' + (row.HouseID + "," + row.ShareHouseID),
                    valueField: 'HouseID', textField: 'Name'
                });
                $("#BHouseID").combobox("setValue", row.HouseID);
                showthdGrid();
                $('#thds').datagrid('clearSelections');
                var gridOpts = $('#thds').datagrid('options');
                gridOpts.url = 'financeApi.aspx?method=QueryReturnOrderDetail&OrderNo=' + row.OrderNo;
            }
        }
        //aa.ProductID ,d.ProductCode,e.ProductName,d.GoodsCode,d.TypeID
        function showthdGrid() {
            $('#thds').datagrid({
                width: '100%', height: '290px',
                loadMsg: '数据加载中请稍候...',
                url: null,
                fitColumns: true,   // 自动适配列宽，推荐加上
                singleSelect: false,
                rownumbers: true,
                idField: 'OrderID',
                columns: [[
                    //{ title: '选择', field: 'OrderID', checkbox: true, width: 60 },
                    { title: '订单号', field: 'OrderNo', width: '120px' },
                    { title: '所在仓库', field: 'HouseName', width: '60px' },
                    { title: '产品编码', field: 'ProductCode', width: '60px' },
                    { title: '货品编码', field: 'GoodsCode', width: '60px' },
                    { title: '货位代码', field: 'ContainerCode', width: '60px' },
                    { title: '产品品牌', field: 'TypeName', width: '60px' },
                    { title: '客户名称', field: 'ClientName', width: '120px' },
                    { title: '数量', field: 'Piece', width: '60px' },
                    { title: '超期费', field: 'OverDueFee', width: '60px' },
                    { title: '销售收入', field: 'ActSalePrice', width: '60px' },
                    { title: '配送费', field: 'TransitFee', width: '60px' },
                    { title: '其他费用', field: 'OtherFee', width: '60px' },
                    { title: '合计', field: 'TotalCharge', width: '60px' },
                    //{
                    //    title: '订单类型', field: 'OrderModel', width: '100px', formatter: function (val) {
                    //        if (val == "0") {
                    //            return "客户单";
                    //        } else if (val == "1") {

                    //            return "退货单";
                    //        }
                    //        return "";
                    //    }
                    //},
                    {
                        title: '退货数量', field: 'ReturnPiece', width: '60px', editor: 'numberbox',
                        styler: function (value, row, index) {
                            return 'background-color:#ffee00;color:red;';
                        }
                    },
                ]],
                onClickCell: onClickCell, // 点击单元格编辑
                onAfterEdit: function (index, row, changes) {
                    // 输入框自动消失，表格恢复普通文本
                    if (parseInt(row.Piece) < parseInt(row.ReturnPiece)) {
                        $('#thds').datagrid('updateRow', {
                            index: index,
                            row: {
                                ReturnPiece: parseInt(row.Piece),
                            }
                        });
                    }
                },
                onCancelEdit: function (index, row) {
                    //console.log('取消编辑:', index, row);
                }
            })
        }

        $.extend($.fn.datagrid.methods, {
            editCell: function (jq, param) {
                return jq.each(function () {
                    var opts = $(this).datagrid('options');
                    var fields = $(this).datagrid('getColumnFields', true).concat(
                        $(this).datagrid('getColumnFields'));
                    for (var i = 0; i < fields.length; i++) {
                        var col = $(this).datagrid('getColumnOption', fields[i]);
                        col.editor1 = col.editor;
                        if (fields[i] != param.field) {
                            col.editor = null;
                        }
                    }
                    $(this).datagrid('beginEdit', param.index);
                    for (var i = 0; i < fields.length; i++) {
                        var col = $(this).datagrid('getColumnOption', fields[i]);
                        col.editor = col.editor1;
                    }
                });
            }
        })


        var editIndex = undefined;
        function onClickCell(index, field) {
            if (endEditing()) {
                $('#thds').datagrid('selectRow', index).datagrid('editCell', {
                    index: index,
                    field: field
                });
                editIndex = index;
                var ed = $(this).datagrid('getEditor', { index: index, field: field });
                if (ed) {
                    console.log(ed, ed.target.next())
                    // 监听输入变化
                    $(ed.target.next()).numberbox({
                        onChange: function (newValue, oldValue) {
                            // 获取当前行数据
                            var rows = $('#thds').datagrid('getRows');
                            var row = rows[index];
                            if (field === 'ReturnPiece') {
                                if (parseInt(row.Piece) < parseInt(newValue)) {
                                    $('#thds').datagrid('updateRow', {
                                        index: index,
                                        row: {
                                            ReturnPiece: parseInt(row.Piece),
                                        }
                                    });
                                } else {
                                    $('#thds').datagrid('updateRow', {
                                        index: index,
                                        row: {
                                            ReturnPiece: parseInt(newValue),
                                        }
                                    });
                                }
                            }

                            //// 举例：输入数量时，自动计算金额 = 数量 * 单价
                            //if (field === 'Quantity') {
                            //    row.Amount = (parseFloat(newValue) || 0) * (row.Price || 0);

                            //    // 更新整行
                            //    $('#dg').datagrid('updateRow', {
                            //        index: index,
                            //        row: row
                            //    });
                            //}
                        }
                    });
                }
            }

        }

        //结束编辑 
        function endEditing() {
            if (editIndex == undefined) {
                return true
            }
            if ($('#thds').datagrid('validateRow', editIndex)) {
                $('#thds').datagrid('endEdit', editIndex);
                editIndex = undefined;
                return true;
            } else {
                return false;
            }
        }

        function AllReturnSum() {
            var list = $("#thds").datagrid('getRows')
            for (var i = 0; i < list.length; i++) {
                $('#thds').datagrid('updateRow', {
                    index: i,
                    row: {
                        ReturnPiece: parseInt(list[i].Piece),
                    }
                });
            }
        }

        //保存退货信息
        function saveItem() {
            var Piece = $('#OrderNum').numberbox('getValue');
            var AwbStatus = $("input[name=AwbStatus]:checked").attr("value")
            var BHouseID = $('#BHouseID').combobox('getValue');
            var OutHouseName = $('#BHouseID').combobox('getText');
            var row = $('#dg').datagrid('getSelected');
            var thdrow = $('#thds').datagrid('getRows');
            if (thdrow.length == 0) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '无可退数据行！', 'warning');
                return;
            }
            if (!BHouseID) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择仓库！', 'warning');
                return;
            }

            var returnPriceSum = 0;
            for (var i = 0; i < thdrow.length; i++) {
                if (thdrow[i].ReturnPiece) {
                    returnPriceSum += thdrow[i].ReturnPiece
                }
            }
            if (returnPriceSum <= 0) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请填写退货数量！', 'warning');
                return;
            }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', "确定生成退货单吗？", function (r) {
                if (r) {
                    $.messager.progress({ msg: '请稍后,正在提交中...' });
                    var list = [];
                    for (var i = 0; i < thdrow.length; i++) {
                        if (parseInt(thdrow[i].ReturnPiece) > 0) {
                            list.push({
                                WXID: row.ID,
                                CaoroOrderID: thdrow[i].OrderID,
                                InCargoStatus: row.InCargoStatus,
                                FacOrderNo: row.FacOrderNo,
                                RefundCheckStatus: row.RefundCheckStatus,
                                CargoOrderNo: thdrow[i].OrderNo,
                                OrderNo: row.OrderNo,
                                WXPayOrderNo: row.WXPayOrderNo,
                                OutHouseID: BHouseID,//退货仓
                                OutHouseName: OutHouseName,//退货仓
                                HouseID: thdrow[i].HouseID,//明细仓库
                                AreaID: thdrow[i].AreaID,//明细仓库
                                ReturnPiece: thdrow[i].ReturnPiece,//ReturnPiece,
                                Piece: thdrow[i].Piece,
                                ActSalePrice: thdrow[i].ActSalePrice,
                                OverDueFee: thdrow[i].OverDueFee,
                                IsTransitFee: AwbStatus,
                                TotalCharge: row.TotalCharge,
                                TransitFee: row.TransitFee,
                                ProductID: thdrow[i].ProductID,
                                ProductCode: thdrow[i].ProductCode,
                                ContainerCode: thdrow[i].ContainerCode,
                                GoodsCode: thdrow[i].GoodsCode,
                                TypeID: row.TypeID,
                            })
                        }
                    }
                    const totalSum = thdrow.reduce((sum, u) => sum + parseInt(u.Piece), 0);
                    const Sum = thdrow.reduce((sum, u) => sum + parseInt(u.ReturnPiece), 0);
                    var json = JSON.stringify(list)
                    $.ajax({
                        url: 'financeApi.aspx?method=AddReturnOrder',
                        type: 'post', dataType: 'json',
                        data: {
                            list: json,
                            IsAllReturn: Sum >= totalSum
                        },
                        success: function (text) {
                            if (text.Result == true) {
                                $.messager.progress("close");
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存成功!', 'info');
                                $('#dlg').dialog('close'); 	// close the dialog
                                dosearch();
                            }
                            else {
                                $.messager.progress("close");
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '保存失败：' + text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        //提交审核
        function submitApprove() {
            var row = $('#dg').datagrid('getSelected');
            var title = "确定审核通过吗？";
            if (row == null || row == "" || row == undefined) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要审核的数据！', 'warning');
                return;
            }
            else if (row.InCargoStatus != "" && row.InCargoStatus != "1" && (row.FacOrderNo != null || row.FacOrderNo != "" || row.FacOrderNo != undefined)) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '该订单的退货单还未完全入库！', 'warning');
                return;
            } else if (row.PayStatus != "2" || row.RefundCheckStatus != "0") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请仔细检查审核状态再审核！', 'warning');
                return;
            } else if (row.OrderStatus != "0" && (row.InCargoStatus == null || row.InCargoStatus == "" || row.InCargoStatus == undefined)) {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '该订单已出库，请先生成退货单！', 'warning');
                return;
            }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', title, function (r) {
                if (r) {
                    $.messager.progress({ msg: '请稍后,正在提交中...' });
                    var json = JSON.stringify(row)
                    $.ajax({
                        url: 'financeApi.aspx?method=UpdateRefundCheckStatus',
                        type: 'post', dataType: 'json',
                        data: {
                            ID: row.ID,
                            RefundCheckStatus: row.RefundCheckStatus,
                            CargoOrderNo: row.CargoOrderNo,
                            OrderID: row.OrderID,
                            OrderNo: row.OrderNo,
                            FacOrderNo: row.FacOrderNo,
                            InCargoStatus: row.InCargoStatus,
                        },
                        success: function (text) {
                            $.messager.progress("close");
                            if (text.Result == true) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '提交成功！', 'info');
                                $('#dg').datagrid('reload');
                            }
                            else {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', text.Message, 'warning');
                            }
                        }
                    });
                }
            });
        }
        //弹出撤销申请备注框
        function repealItem() {
            var rows = $('#dg').datagrid('getSelections')[0];
            if (rows == null || rows == "") {
                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要撤销的数据！', 'warning'); return;
                if (rows.RefundCheckStatus) {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要撤销的数据！', 'warning'); return;
                }
                if (rows.length > 1) {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择一行数据！', 'warning'); return;
                }
                rows = $('#dg').datagrid('getSelected');
                if (rows == null || rows == "") {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要撤销的数据！', 'warning'); return;
                }

            }
            if (rows) {
                if (rows.RefundCheckStatus != 0) {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '审核状态:未审核 才能进行撤销！', 'warning'); return;
                }

                if (rows.PayStatus != 2) {
                    $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '付款状态:申请退款 才能进行撤销！', 'warning'); return;
                }

                $('#dlgRepealItem').dialog('open').dialog('setTitle', '撤销微信订单号：' + rows.OrderNo);
                $('#Remark').numberbox('setValue', '');
                $('#Remark').numberbox().next('span').find('input').focus();
            }

        }
        //取消退款申请
        function saveRepealItem() {
            var rows = $('#dg').datagrid('getSelected');
            console.log("1111111111111", rows)
            //return
            if (rows == null || rows == "") { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '请选择要撤销的数据！', 'warning'); return; }
            $.messager.confirm('<%= Cargo.Common.GetSystemNameAndVersion()%>', '确定撤销？', function (r) {
                if (r) {
                    var json = JSON.stringify(rows);
                    $('#fmRepealItem').form('submit', {
                        url: 'financeApi.aspx?method=saveRepealItem',
                        contentType: "application/json;charset=utf-8", dataType: "json",
                        data: {
                            ID: rows.ID,
                            RefundCheckStatus: rows.RefundCheckStatus,
                            CargoOrderNo: rows.CargoOrderNo,
                            OrderID: rows.OrderID,
                            OrderNo: rows.OrderNo,
                            FacOrderNo: rows.FacOrderNo,
                            InCargoStatus: rows.InCargoStatus,
                        },
                        onSubmit: function (param) {
                            param.submitData = json;
                            var trd = $(this).form('enableValidation').form('validate');
                            return trd;
                        },
                        success: function (msg) {
                            var result = eval('(' + msg + ')');
                            if (result.Result) {
                                $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '撤销成功!', 'info');
                                $('#dlgRepealItem').dialog('close');
                                dosearch();
                            } else { $.messager.alert('<%= Cargo.Common.GetSystemNameAndVersion()%>', '撤销失败：' + result.Message, 'warning'); }
                        }
                    })
                }
            });
        }

    </script>
</asp:Content>