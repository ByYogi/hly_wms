    /// <summary>
    /// 仓库接口返回数据实体
    /// </summary>
    [Serializable]
    public class CargoAPIMessage
    {
        /// <summary>
        /// True：返回成功，False：返回失败
        /// </summary>
        public bool Result { get; set; }
        /// <summary>
        /// 成功或失败的信息
        /// </summary>
        public string Message { get; set; }
        /// <summary>
        /// 订单号
        /// </summary>
        public string OrderNo { get; set; }
        /// <summary>
        /// 库存数
        /// </summary>
        public int Number { get; set; }
        /// <summary>
        /// 出发站
        /// </summary>
        public string Dep { get; set; }
        /// <summary>
        /// 到达站
        /// </summary>
        public string Dest { get; set; }
        /// <summary>
        /// 收货人
        /// </summary>
        public string AcceptPeople { get; set; }
        /// <summary>
        /// 收货人电话
        /// </summary>
        public string AcceptCellphone { get; set; }
        /// <summary>
        /// 收货人地址
        /// </summary>
        public string AcceptAddress { get; set; }
        /// <summary>
        /// 好来运运输单号
        /// </summary>
        public string HLYAwbNo { get; set; }
        /// <summary>
        /// 扫描数量
        /// </summary>
        public int ScanNum { get; set; }
        /// <summary>
        /// 库存表主键
        /// </summary>
        public long ContainerGoodsID { get; set; }
        public int PitNum { get; set; }
        public int SumPiece { get; set; }
        public int SumScanPiece { get; set; }

        public CargoProductInfoEntity ProductTagInfo { get; set; }
        /// <summary>
        /// 返回库存产品数据实体List
        /// </summary>
        public List<CargoInterfaceEntity> cargoList { get; set; }
        public List<CargoOrderPickPlanGoodsEntity> orderPickPlanList { get; set; }

        public Cargo.CargoGtmcProOrderEntity CargoGtmcProOrderEntity { get; set; }
    }
